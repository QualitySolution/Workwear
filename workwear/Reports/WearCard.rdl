        </Field>
        <Field Name="last_issue">
          <DataField>last_issue</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="received">
          <DataField>received</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="in_stock">
          <DataField>in_stock</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
