  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    item_types.name AS type_name,
    item_types.id AS type_id,
    CASE item_types.wear_category
        WHEN 'Wear' THEN wear_cards.wear_growth
        ELSE NULL
    END AS growth,
    CASE item_types.wear_category
        WHEN 'Wear' THEN wear_cards.size_wear
        WHEN 'Shoes' THEN wear_cards.size_shoes
        WHEN 'Headgear' THEN wear_cards.size_headdress
        WHEN 'Gloves' THEN wear_cards.size_gloves
        ELSE NULL
    END AS size,
    measurement_units.name AS unit,
    measurement_units.okei AS okei,
    SUM(CASE norms_item.period_type
        WHEN 'Year' THEN norms_item.amount / norms_item.period_count
        WHEN 'Month' THEN norms_item.amount * 12 / norms_item.period_count
        ELSE NULL
    END) AS amount_sum
FROM
    wear_cards_item
        LEFT JOIN
    wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
        LEFT JOIN
    item_types ON item_types.id = wear_cards_item.itemtype_id
        LEFT JOIN
    measurement_units ON item_types.units_id = measurement_units.id
        LEFT JOIN
    norms_item ON wear_cards_item.norm_item_id = norms_item.id
WHERE wear_cards.dismiss_date IS NULL
GROUP BY item_types.id , size , growth
ORDER BY type_name , size , growth</CommandText>
      </Query>
      <Fields >
        <Field Name="type_name">
          <DataField>type_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="type_id">
          <DataField>type_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <TypeName>System.String</TypeName>
