  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    item_types.name AS name_siz,
    income_detail.certificate,
    stock_expense.date AS expense_date,
    stock_expense_detail.quantity AS expense_quantity,
    ROUND(100 - income_detail.life_percent * 100) AS wearout,
    CONCAT_WS('\n',
            GROUP_CONCAT(DATE_FORMAT(returned.date, '%d.%m.%Y')
                SEPARATOR '\n'),
            IF(stock_expense_detail.auto_writeoff_date &lt;= CURDATE()
                    AND stock_expense_detail.quantity - IFNULL(SUM(returned.quantity), 0) &gt; 0,
                DATE_FORMAT(stock_expense_detail.auto_writeoff_date,
                        '%d.%m.%Y'),
                NULL)) AS returned_date,
    CONCAT_WS('\n',
            GROUP_CONCAT(returned.quantity
                SEPARATOR '\n'),
            IF(stock_expense_detail.auto_writeoff_date &lt;= CURDATE()
                    AND stock_expense_detail.quantity - IFNULL(SUM(returned.quantity), 0) &gt; 0,
                stock_expense_detail.quantity - IFNULL(SUM(returned.quantity), 0),
                NULL)) AS returned_quantity,
    CONCAT_WS('\n',
            GROUP_CONCAT(ROUND(IF(returned.life_percent = - 1, IF(returned.date &gt;= stock_expense_detail.auto_writeoff_date, 100,
                            IFNULL(
                    DATEDIFF(returned.date, stock_expense.date) /
                            DATEDIFF(stock_expense_detail.auto_writeoff_date, stock_expense.date)
                            * 100, 100)),
                    100 - returned.life_percent * 100))
                SEPARATOR '\n'),
            IF(stock_expense_detail.auto_writeoff_date &lt;= CURDATE()
                    AND stock_expense_detail.quantity - IFNULL(SUM(returned.quantity), 0) &gt; 0,
                100,
                NULL)) AS returned_wearout
FROM
    stock_expense
        LEFT JOIN
    stock_expense_detail ON stock_expense_detail.stock_expense_id = stock_expense.id
        LEFT JOIN
    nomenclature ON stock_expense_detail.nomenclature_id = nomenclature.id
        LEFT JOIN
    item_types ON item_types.id = nomenclature.type_id
        LEFT JOIN
    stock_income_detail AS income_detail ON stock_expense_detail.stock_income_detail_id = income_detail.id
        LEFT JOIN
    (SELECT 
        return_detail.stock_expense_detail_id,
            return_doc.date,
            return_detail.quantity,
            return_detail.life_percent
    FROM
        stock_income_detail AS return_detail
    LEFT JOIN stock_income AS return_doc ON return_detail.stock_income_id = return_doc.id UNION SELECT 
        stock_write_off_detail.stock_expense_detail_id,
            stock_write_off.date,
            stock_write_off_detail.quantity,
            - 1 AS life_percent
    FROM
        stock_write_off_detail
    LEFT JOIN stock_write_off ON stock_write_off.id = stock_write_off_detail.stock_write_off_id) AS returned ON returned.stock_expense_detail_id = stock_expense_detail.id
WHERE
    stock_expense.wear_card_id = @id
GROUP BY stock_expense_detail.id
ORDER BY expense_date;</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name_siz">
          <DataField>name_siz</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="certificate">
          <DataField>certificate</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="expense_date">
          <DataField>expense_date</DataField>
