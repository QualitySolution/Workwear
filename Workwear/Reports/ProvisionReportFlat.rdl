  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT *,
       ROUND(tabel.balance / tabel.need * 100, 0) AS 'provision_percent'
FROM (SELECT
          IF(@group_by_subdivision, wear_cards.object_id, NULL) AS subdivision_id,
          IF(@group_by_subdivision, objects.name, NULL) AS subdivision_name,
          protection_tools.id AS 'protection_tools_id',
          protection_tools.name AS 'protection_tools_name',
          if(@show_sex,
             IF(wear_cards.sex = 'M','лю', IF(wear_cards.sex = 'F','лќ','')),
             null)
          AS 'employee_sex',
          if(@show_stock, sizeAlias.name, null) as size,
          if(@show_size, heightAlias.name, null) as height,
          SUM(norms_item.amount) AS 'need',
          SUM(
                  LEAST(
                      norms_item.amount,
                      IFNULL(
                              (SELECT
                                   SUM(
                                       o.issued - IFNULL(
                                           (SELECT SUM(ret.returned)
                                            FROM operation_issued_by_employee ret
                                            WHERE ret.issued_operation_id = o.id),
                                           0)
                                   )
                              FROM operation_issued_by_employee o
                              WHERE o.employee_id = wear_cards_item.wear_card_id
                                AND o.protection_tools_id = wear_cards_item.protection_tools_id
                                AND o.auto_writeoff_date &gt;= @report_date),
                              0)
                      )
              ) as balance,
          IF(@show_stock,
              IFNULL(
                   (SELECT(
                      SUM(
                         CASE WHEN operation_warehouse.warehouse_receipt_id IS NOT NULL THEN operation_warehouse.amount ELSE 0
                                END
                         ) - SUM(
                             CASE WHEN operation_warehouse.warehouse_expense_id IS NOT NULL THEN operation_warehouse.amount ELSE 0
                                END
                         ))
                   FROM
                       operation_warehouse
                   WHERE  
                           CONVERT(operation_warehouse.operation_time, DATE) &lt;= CONVERT(@report_date, DATE) AND operation_warehouse.nomenclature_id IN(
                       SELECT
                           protection_tools_nomenclature.nomenclature_id
                       FROM
                           protection_tools_nomenclature
                               JOIN nomenclature n ON n.id = protection_tools_nomenclature.nomenclature_id
                       WHERE 
                           (n.sex = 'Universal' OR (n.sex = 'Men' AND wear_cards.sex = 'M') OR (n.sex = 'Women' AND wear_cards.sex = 'F'))
                         AND protection_tools_nomenclature.protection_tools_id = wear_cards_item.protection_tools_id
                   )
                     AND (sizeTypeAlias.id IS NULL OR operation_warehouse.size_id IS NULL OR operation_warehouse.size_id = employeeSizeAlias.size_id OR operation_warehouse.size_id IN (SELECT IF(size_suitable.size_id = employeeSizeAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeSizeAlias.size_id OR size_suitable.`size_suitable_id` = employeeSizeAlias.size_id))
                     AND (heightTypeAlias.id IS NULL OR operation_warehouse.height_id IS NULL OR operation_warehouse.height_id = employeeHeightAlias.size_id OR operation_warehouse.height_id IN (SELECT IF(size_suitable.size_id = employeeHeightAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeHeightAlias.size_id OR size_suitable.`size_suitable_id` = employeeHeightAlias.size_id))
                  
                    )    
                , 0)
              ,null) AS in_stock,
          IF(@show_employees,
          GROUP_CONCAT( CONCAT_WS('',
              CONCAT(wear_cards.last_name, ' '),
              IF(wear_cards.first_name = '',
                 NULL,
                 CONCAT(LEFT(wear_cards.first_name, 1), '.')),
              IF(wear_cards.patronymic_name = '',
                 NULL,
                 CONCAT(LEFT(wear_cards.patronymic_name, 1), '.'))) SEPARATOR ', ' LIMIT 1000)
          ,null) AS employees
      FROM wear_cards_item
               JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
               LEFT JOIN objects ON wear_cards.object_id = objects.id
               JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
               LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
               LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
               LEFT JOIN wear_cards_vacations ON wear_cards.id = wear_cards_vacations.wear_card_id
               LEFT JOIN size_types AS sizeTypeAlias ON sizeTypeAlias.id = item_types.size_type_id
               LEFT JOIN size_types AS heightTypeAlias ON heightTypeAlias.id = item_types.height_type_id
               LEFT JOIN wear_cards_sizes AS employeeSizeAlias
                         ON (employeeSizeAlias.employee_id = wear_cards_item.wear_card_id AND
                             employeeSizeAlias.size_type_id = sizeTypeAlias.id)
               LEFT JOIN sizes AS sizeAlias ON sizeAlias.id = employeeSizeAlias.size_id
               LEFT JOIN wear_cards_sizes AS employeeHeightAlias
                         ON (employeeHeightAlias.employee_id = wear_cards_item.wear_card_id AND
                             employeeHeightAlias.size_type_id = heightTypeAlias.id)
               LEFT JOIN sizes AS heightAlias ON heightAlias.id = employeeHeightAlias.size_id
               JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
      WHERE
          wear_cards.dismiss_date IS NULL
        #AND protection_tools.id IN (@protection_tools_ids)
        #AND ((@without_subdivision AND wear_cards.object_id IS NULL) OR (wear_cards.object_id IN (@subdivision_ids)))
        AND (NOT @exclude_in_vacation
          OR (wear_cards_vacations.begin_date IS NULL
              OR (CONVERT(@report_date, DATE) BETWEEN
                  wear_cards_vacations.begin_date AND wear_cards_vacations.end_date
                  )
                 )
          )
      GROUP BY
          wear_cards_item.protection_tools_id,
          IF(@group_by_subdivision, subdivision_id, NULL), 
          IF(@show_sex, employee_sex, NULL),
          IF(@show_size, employeeSizeAlias.size_id, NULL),
          IF(@show_size, employeeHeightAlias.size_id, NULL)
      ORDER BY
          IF(@group_by_subdivision, subdivision_name, NULL),
          protection_tools.name,
          IF(@show_sex, employee_sex, NULL),
          IF(@show_size, size, NULL),
          IF(@show_size, height, NULL)
     ) as tabel</CommandText>
        <QueryParameters>
          <QueryParameter Name="report_date">
            <Value>=Parameters!report_date</Value>
          </QueryParameter>
          <QueryParameter Name="show_sex">
            <Value>=Parameters!show_sex</Value>
          </QueryParameter>
          <QueryParameter Name="show_size">
            <Value>=Parameters!show_size</Value>
          </QueryParameter>
          <QueryParameter Name="subdivision_ids">
            <Value>=Parameters!subdivision_ids</Value>
          </QueryParameter>
          <QueryParameter Name="protection_tools_ids">
            <Value>=Parameters!protection_tools_ids</Value>
          </QueryParameter>
          <QueryParameter Name="group_by_subdivision">
            <Value>=Parameters!group_by_subdivision</Value>
          </QueryParameter>
          <QueryParameter Name="exclude_in_vacation">
            <Value>=Parameters!exclude_in_vacation</Value>
          </QueryParameter>
          <QueryParameter Name="without_subdivision">
            <Value>=Parameters!without_subdivision</Value>
          </QueryParameter>
          <QueryParameter Name="show_stock">
            <Value>=Parameters!show_stock</Value>
          </QueryParameter>
          <QueryParameter Name="show_employees">
            <Value>=Parameters!show_employees</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
