  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
   'СИБУР' as 'company',   
    wear_cards.personnel_number as 'tab_num',
    CONCAT_WS(' ',wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name) as "employee",
    objects.name as 'division',
    departments.name as 'department',
    posts.name as 'post',
    IF(s_1.id IS NULL, '-', s_1.name) as 'size_name_1',
    IF(s_2.id IS NULL, '-', s_2.name) as 'size_name_2',
    IF(s_3.id IS NULL, '-', s_3.name) as 'size_name_3',
    IF(s_4.id IS NULL, '-', s_4.name) as 'size_name_4',
    IF(s_5.id IS NULL, '-', s_5.name) as 'size_name_5',
    IF(s_6.id IS NULL, '-', s_6.name) as 'size_name_6',
    IF(s_9.id IS NULL, '-', s_9.name) as 'size_name_9',
    IF(s_7.id IS NULL, '-', s_7.name) as 'size_name_7',
    IF(s_8.id IS NULL, '-', s_8.name)as 'size_name_8',
    IF(s_10.id IS NULL, '-', s_10.name) as 'size_name_10',
    IF(s_11.id IS NULL, '-', s_11.name) as 'size_name_11',
    IF(s_12.id IS NULL, '-', s_12.name) as 'size_name_12',
    sex
FROM wear_cards
        LEFT JOIN objects on wear_cards.object_id = objects.id
        LEFT JOIN departments on wear_cards.department_id = departments.id
        LEFT JOIN posts on wear_cards.post_id = posts.id    
        LEFT JOIN wear_cards_sizes as wci_1 on (wci_1.employee_id = wear_cards.id AND wci_1.size_type_id = 1)
        LEFT JOIN sizes as s_1 on wci_1.size_id = s_1.id
        LEFT JOIN wear_cards_sizes as wci_2 on (wci_2.employee_id = wear_cards.id AND wci_2.size_type_id = 2)
        LEFT JOIN sizes as s_2 on wci_2.size_id = s_2.id
        LEFT JOIN wear_cards_sizes as wci_3 on (wci_3.employee_id =
wear_cards.id AND wci_3.size_type_id = 3)
		LEFT JOIN sizes as s_3 on wci_3.size_id = s_3.id
        LEFT JOIN wear_cards_sizes as wci_4 on (wci_4.employee_id = wear_cards.id AND wci_4.size_type_id = 4)
        LEFT JOIN sizes as s_4 on wci_4.size_id = s_4.id
        LEFT JOIN wear_cards_sizes as wci_5 on (wci_5.employee_id =
wear_cards.id AND wci_5.size_type_id = 5)
		LEFT JOIN sizes as s_5 on wci_5.size_id = s_5.id
        LEFT JOIN wear_cards_sizes as wci_6 on (wci_6.employee_id =
wear_cards.id AND wci_6.size_type_id = 6)
		LEFT JOIN sizes as s_6 on wci_6.size_id = s_6.id
        LEFT JOIN wear_cards_sizes as wci_7 on (wci_7.employee_id = wear_cards.id AND wci_7.size_type_id = 7)
        LEFT JOIN sizes as s_7 on wci_7.size_id = s_7.id
        LEFT JOIN wear_cards_sizes as wci_8 on (wci_8.employee_id = wear_cards.id AND wci_8.size_type_id = 8)
        LEFT JOIN sizes as s_8 on wci_8.size_id = s_8.id
        LEFT JOIN wear_cards_sizes as wci_9 on (wci_9.employee_id = wear_cards.id AND wci_9.size_type_id = 9)
        LEFT JOIN sizes as s_9 on wci_9.size_id = s_9.id
        LEFT JOIN wear_cards_sizes as wci_10 on (wci_10.employee_id = wear_cards.id AND wci_10.size_type_id = 10)
        LEFT JOIN sizes as s_10 on wci_10.size_id = s_10.id
        LEFT JOIN wear_cards_sizes as wci_11 on (wci_11.employee_id = wear_cards.id AND wci_11.size_type_id = 11)
        LEFT JOIN sizes as s_11 on wci_11.size_id = s_11.id
        LEFT JOIN wear_cards_sizes as wci_12 on (wci_12.employee_id = wear_cards.id AND wci_12.size_type_id = 12)
        LEFT JOIN sizes as s_12 on wci_12.size_id = s_12.id
WHERE
    wear_cards.dismiss_date is null;</CommandText>
      </Query>
