  <DataSources>
    <DataSource Name="DS1">
      <ConnectionProperties>
        <DataProvider>MySQL.NET</DataProvider>
        <ConnectString>server=10.0.2.2;user=admin;database=workwear;port=3306;password=123;</ConnectString>
        <IntegratedSecurity>false</IntegratedSecurity>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT objects.code, objects.name as obj,SUM(operation.peronal_op) as count_peronal, SUM(operation.collective_op) as count_collective
	FROM (SELECT DISTINCT employee_id, IF(item_types.issue_type = "Personal",1,0) AS peronal_op, IF(item_types.issue_type = "Collective",1,0) AS collective_op
          FROM operation_issued_by_employee 
          	JOIN protection_tools ON protection_tools.id = operation_issued_by_employee.protection_tools_id
    		LEFT JOIN item_types on item_types.id= protection_tools.item_types_id
 		  WHERE (operation_time &gt; CONVERT(@dateStart, DATE) AND operation_time &lt; DATE_ADD(CONVERT(@dateEnd, DATE),INTERVAL 1 DAY )) ) AS operation 
          
	JOIN wear_cards ON employee_id = wear_cards.id  
	LEFT JOIN objects ON wear_cards.object_id = objects.id 
	GROUP BY objects.id 
    ORDER BY objects.code</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateStart">
            <Value>=Format({?dateStart}, "yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="dateEnd">
            <Value>=Format({?dateEnd}, " yyyy-MM-dd")</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="code">
          <DataField>code</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="obj">
          <DataField>obj</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="count_peronal">
          <DataField>count_peronal</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="count_collective">
          <DataField>count_collective</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
