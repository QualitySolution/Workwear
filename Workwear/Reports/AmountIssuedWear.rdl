  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select 
    if(@summary, NULL, objects.id) as subdivision_id, 
    if(@summary, NULL, objects.name) as object, 
    IF(@byEmployee, wear_cards.personnel_number, NULL) AS personnel_number,
    IF(@byEmployee, CONCAT_WS(' ', wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name), NULL) AS employee,
    IF(@byEmployee, posts.name, NULL) AS post,
    nomenclature.name as nomen, 
    sum(issued) as sum, 
    nomenclature.number, 
    if(@alternativeName, IFNULL(sizeAlias.alternative_name, sizeAlias.name), sizeAlias.name) as size, 
    heightAlias.name as growth, 
    objects.code, 
    if(@showOwners, owners.name, null) as owner_name 
FROM operation_issued_by_employee
LEFT JOIN wear_cards on wear_cards.id = operation_issued_by_employee.employee_id
LEFT JOIN objects on objects.id = wear_cards.object_id
LEFT JOIN posts ON posts.id = wear_cards.post_id
LEFT JOIN nomenclature on nomenclature.id = operation_issued_by_employee.nomenclature_id
LEFT JOIN item_types ON item_types.id = nomenclature.type_id
LEFT JOIN sizes as sizeAlias ON (sizeAlias.id = operation_issued_by_employee.size_id)
LEFT JOIN sizes AS heightAlias ON (heightAlias.id = operation_issued_by_employee.height_id)
LEFT JOIN operation_warehouse ON warehouse_operation_id = operation_warehouse.id
LEFT JOIN owners ON operation_warehouse.owner_id = owners.id
WHERE operation_issued_by_employee.operation_time &gt;= CONVERT(@dateStart, DATE) and operation_issued_by_employee.operation_time &lt;= CONVERT(@dateEnd, DATE)
AND NOT operation_issued_by_employee.manual_operation -- В отчете не показываем ручные операции. 
AND (@issue_type = "" OR item_types.issue_type = @issue_type)
and operation_issued_by_employee.returned = 0 AND ((@withoutsub AND objects.id IS NULL) OR objects.id IN (@subdivisions))
and (@matchString = '' OR nomenclature.name LIKE CONCAT('%', @matchString, '%'))
and (@noMatchString = '' OR nomenclature.name NOT LIKE CONCAT('%', @noMatchString, '%'))
AND (@showOwners AND (@allOwners OR ((@withoutOwner AND owners.id IS NULL) OR owners.id = @ownerId)))
GROUP BY subdivision_id, IF(@byEmployee, wear_cards.id, NULL), operation_issued_by_employee.nomenclature_id, IF(@bysize, size, NULL), IF(@bysize, growth, NULL), IF(@showOwners, owners.id, NULL)
ORDER BY CAST(objects.code AS UNSIGNED), employee, nomen, size, growth;</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateStart">
            <Value>=Format({?dateStart}, "yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="dateEnd">
            <Value>=Format({?dateEnd}, "yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="withoutsub">
            <Value>={?withoutsub}</Value>
          </QueryParameter>
          <QueryParameter Name="subdivisions">
            <Value>={?subdivisions}</Value>
          </QueryParameter>
          <QueryParameter Name="summary">
            <Value>={?summary}</Value>
          </QueryParameter>
          <QueryParameter Name="bysize">
            <Value>={?bySize}</Value>
          </QueryParameter>
          <QueryParameter Name="issue_type">
            <Value>={?issue_type}</Value>
          </QueryParameter>
          <QueryParameter Name="matchString">
            <Value>={?matchString}</Value>
          </QueryParameter>
          <QueryParameter Name="noMatchString">
            <Value>={?noMatchString}</Value>
          </QueryParameter>
          <QueryParameter Name="alternativeName">
            <Value>={?alternativeName}</Value>
          </QueryParameter>
          <QueryParameter Name="showOwners">
            <Value>={?showOwners}</Value>
          </QueryParameter>
          <QueryParameter Name="ownerId">
            <Value>={?ownerId}</Value>
          </QueryParameter>
          <QueryParameter Name="withoutOwner">
            <Value>={?withoutOwner}</Value>
          </QueryParameter>
          <QueryParameter Name="allOwners">
            <Value>={?allOwners}</Value>
          </QueryParameter>
          <QueryParameter Name="byEmployee">
            <Value>={?byEmployee}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="subdivision_id">
          <DataField>subdivision_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="object">
          <DataField>object</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomen">
          <DataField>nomen</DataField>
