  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>1500</Timeout>
        <CommandText>SELECT
    wear_cards.id as employee_id,
    CONCAT_WS(
        ' ',
        wear_cards.last_name,
        wear_cards.first_name,
        wear_cards.patronymic_name
    ) AS employee,
    wear_cards.personnel_number,
    wear_cards_item.next_issue,
    (SELECT MAX(operation_issued_by_employee.operation_time) FROM operation_issued_by_employee WHERE operation_issued_by_employee.employee_id = wear_cards.id AND protection_tools.id = operation_issued_by_employee.protection_tools_id AND operation_issued_by_employee.issued &gt; 0) as last_issue,
    IF(
        wear_cards_item.next_issue &lt;= CONVERT(@report_date, DATE),
        DATEDIFF(
            CONVERT(@report_date, DATE),
            wear_cards_item.next_issue
        ),
        NULL
    ) AS delay,
    protection_tools.name AS protection_tools_name,
    protection_tools.assessed_cost * norms_item.amount AS protection_tools_assessed_cost,
    wear_cards_item.protection_tools_id,
    objects.id as subdivision_id,
    objects.name as subdivision_name,
    posts.id as post_id,
    posts.name as post_name,
    departments.id as department_id,
    departments.name as department_name,
    sizeTypeAlias.id as sizeTypeId,
    heightTypeAlias.id as heightTypeId,
    sizeAlias.name AS size,
	heightAlias.name AS growth,
    measurement_units.name AS unit,
    norms_item.amount AS amount_by_norm,
    IF(@show_stock, ballance.SUM, null) AS in_stock
FROM
    wear_cards_item
    JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
    JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
    LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
    LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
    JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
    LEFT JOIN norm_conditions ON norms_item.condition_id = norm_conditions.id
    LEFT JOIN departments ON wear_cards.department_id = departments.id
    LEFT JOIN posts ON wear_cards.post_id = posts.id
    LEFT JOIN objects ON wear_cards.object_id = objects.id OR wear_cards.object_id IS NULL AND posts.subdivision_id = objects.id
    LEFT JOIN wear_cards_vacations ON wear_cards.id = wear_cards_vacations.wear_card_id
    LEFT JOIN size_types AS sizeTypeAlias ON sizeTypeAlias.id = item_types.size_type_id
    LEFT JOIN size_types AS heightTypeAlias ON heightTypeAlias.id = item_types.height_type_id
    LEFT JOIN wear_cards_sizes AS employeeSizeAlias ON (employeeSizeAlias.employee_id = wear_cards_item.wear_card_id AND employeeSizeAlias.size_type_id = sizeTypeAlias.id)
    LEFT JOIN sizes AS sizeAlias ON sizeAlias.id = employeeSizeAlias.size_id
    LEFT JOIN wear_cards_sizes AS employeeHeightAlias ON (employeeHeightAlias.employee_id = wear_cards_item.wear_card_id AND employeeHeightAlias.size_type_id = heightTypeAlias.id)
    LEFT JOIN sizes AS heightAlias ON heightAlias.id = employeeHeightAlias.size_id
    LEFT JOIN (SELECT sex_b.ptid, sex_b.size_id, sex_b.height_id, sex_const.sex, 
                   IF(sex_const.sex = 'Men', sex_b.SUM_M + sex_b.SUM_U, sex_b.SUM_F + sex_b.SUM_U) AS SUM
        FROM (
        #Универсальый зарос
            SELECT spt.id AS ptid, sopwh.size_id, sopwh.height_id,
                 (SUM(IF(sn.sex = 'Universal' AND (sopwh.warehouse_receipt_id IS NOT NULL AND
                                                   (@all_warehouse OR sopwh.warehouse_receipt_id = @warehouse_id)), sopwh.amount, 0))
                     - SUM(IF(sn.sex = 'Universal' AND (sopwh.warehouse_expense_id IS NOT NULL AND
                                                        (@all_warehouse OR sopwh.warehouse_expense_id = @warehouse_id)), sopwh.amount, 0))
                     )  AS SUM_U,
                 (SUM(IF(sn.sex = 'Men' AND (sopwh.warehouse_receipt_id IS NOT NULL AND
                                             (@all_warehouse OR sopwh.warehouse_receipt_id = @warehouse_id)), sopwh.amount, 0))
                     - SUM(IF(sn.sex = 'Men' AND (sopwh.warehouse_expense_id IS NOT NULL AND
                                                  (@all_warehouse OR sopwh.warehouse_expense_id = @warehouse_id)), sopwh.amount, 0))
                     )  AS SUM_M,
                 (SUM(IF(sn.sex = 'Women' AND (sopwh.warehouse_receipt_id IS NOT NULL AND
                                               (@all_warehouse OR sopwh.warehouse_receipt_id = @warehouse_id)), sopwh.amount, 0))
                     - SUM(IF(sn.sex = 'Women' AND (sopwh.warehouse_expense_id IS NOT NULL AND
                                                    (@all_warehouse OR sopwh.warehouse_expense_id = @warehouse_id)), sopwh.amount, 0))
                     )  AS SUM_F
              FROM protection_tools AS spt
                   LEFT JOIN protection_tools_nomenclature AS sptn ON spt.id = sptn.protection_tools_id
                   LEFT JOIN nomenclature AS sn ON sptn.nomenclature_id = sn.id
                   LEFT JOIN operation_warehouse AS sopwh
                             on (sopwh.operation_time &lt; ADDDATE(@report_date, INTERVAL 1 DAY)
                                 AND(!@hide_worn OR sopwh.wear_percent = 0.00)
                                 AND sopwh.nomenclature_id = sptn.nomenclature_id
                                 AND (@all_warehouse
                                     OR sopwh.warehouse_receipt_id = @warehouse_id
                                     OR sopwh.warehouse_expense_id = @warehouse_id))
              WHERE ((-1 IN (@protection_tools_ids)) OR spt.id IN (@protection_tools_ids))
              GROUP BY spt.id, sopwh.size_id, sopwh.height_id
        #Конец универсального зарпроса      
              ) AS sex_b
            JOIN (SELECT IF(const.seq = 1,'Men','Women')  as sex FROM seq_1_to_2 as const) AS sex_const
GROUP BY sex_const.sex, sex_b.ptid, sex_b.size_id, sex_b.height_id) 
        AS ballance 
            ON (wear_cards_item.protection_tools_id = ballance.ptid
            AND (ballance.sex = 'Universal' OR (ballance.sex = 'Men' AND wear_cards.sex = 'M') OR (ballance.sex = 'Women' AND wear_cards.sex = 'F'))
            AND (sizeTypeAlias.id IS NULL OR ballance.size_id IS NULL OR ballance.size_id = employeeSizeAlias.size_id OR ballance.size_id IN (SELECT IF(size_suitable.size_id = employeeSizeAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeSizeAlias.size_id OR size_suitable.`size_suitable_id` = employeeSizeAlias.size_id))
            AND (heightTypeAlias.id IS NULL OR ballance.height_id IS NULL OR ballance.height_id = employeeHeightAlias.size_id OR ballance.height_id IN (SELECT IF(size_suitable.size_id = employeeHeightAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeHeightAlias.size_id OR size_suitable.`size_suitable_id` = employeeHeightAlias.size_id))
            )     
WHERE
    wear_cards.dismiss_date IS NULL
    AND (@exclude_in_vacation = false OR (CONVERT(@report_date, DATE) NOT BETWEEN wear_cards_vacations.begin_date AND wear_cards_vacations.end_date OR wear_cards_vacations.begin_date IS NULL))
    AND (@subdivision_id = -1 OR wear_cards.object_id = @subdivision_id )
    AND (@department_id = -1 OR wear_cards.department_id = @department_id)
    AND ((-1 IN (@protection_tools_ids)) OR protection_tools.id IN (@protection_tools_ids))
    AND ((-1 IN (@employee_groups_ids)) 
         OR(@without_groups AND NOT EXISTS
             (SELECT * FROM employee_group_items as gi 
               WHERE gi.employee_id = wear_cards_item.wear_card_id) ) 
         OR (EXISTS (SELECT * FROM employee_group_items as gi
               WHERE gi.employee_id = wear_cards_item.wear_card_id
                 AND gi.employee_group_id IN (@employee_groups_ids))) )
    AND (@condition = false OR norm_conditions.issuance_start is null OR
        IF(SUBSTRING(norm_conditions.issuance_start,6) &lt; SUBSTRING(norm_conditions.issuance_end,6),
            SUBSTRING(@report_date,6) &gt;= SUBSTRING(norm_conditions.issuance_start,6) AND SUBSTRING(@report_date,6) &lt;= SUBSTRING(norm_conditions.issuance_end,6),
            SUBSTRING(@report_date,6) &gt;= SUBSTRING(norm_conditions.issuance_start,6) OR SUBSTRING(@report_date,6) &lt;= SUBSTRING(norm_conditions.issuance_end,6)))
    AND( @issue_type = '' OR item_types.issue_type = @issue_type )
    AND wear_cards_item.next_issue &lt; CONVERT(@report_date, DATE)
    AND (@exclude_before = '' OR CONVERT(@exclude_before,DATE) &lt; wear_cards_item.next_issue)
GROUP BY
    wear_cards_item.wear_card_id,
    wear_cards_item.protection_tools_id
HAVING
    (@exclude_zero_stock = false OR in_stock &gt; 0)
ORDER BY
    employee,
    protection_tools_name;</CommandText>
