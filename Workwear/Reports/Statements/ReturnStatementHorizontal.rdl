  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    employees.last_name,
    employees.first_name,
    employees.patronymic_name,
    nomenclature.name as nomenclature_name,
    nomenclature.number as nomenclature_number,
    measurement_units.name as units,
    this_issuse.returned as amount_return,
    sizeAlias.name as size,
    heightAlias.name as growth,
    stock_return_items.comment_return
FROM stock_return_items
         LEFT JOIN stock_return ON stock_return.id = stock_return_items.stock_return_id
         LEFT JOIN employees ON employees.id = stock_return.employee_id
         LEFT JOIN nomenclature on nomenclature.id = stock_return_items.nomenclature_id
         LEFT JOIN operation_issued_by_employee as this_issuse on this_issuse.id = stock_return_items.employee_issue_operation_id
         LEFT JOIN item_types ON nomenclature.type_id = item_types.id
         LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
         LEFT JOIN sizes AS sizeAlias ON sizeAlias.id = stock_return_items.size_id
         LEFT JOIN sizes AS heightAlias ON heightAlias.id = stock_return_items.height_id
WHERE stock_return_items.stock_return_id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="last_name">
          <DataField>last_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="first_name">
          <DataField>first_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="patronymic_name">
          <DataField>patronymic_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_number">
          <DataField>nomenclature_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="units">
          <DataField>units</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount_return">
          <DataField>amount_return</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="comment_return">
          <DataField>comment_return</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Sheet">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    IFNULL(stock_return.doc_number, stock_return.id)
                                                                                                                       as doc_number,
    stock_return.date AS doc_date,
    CONCAT_WS(' - ', ob8.name, ob7.name, ob6.name, ob5.name, ob4.name, ob3.name, ob2.name, ob1.name, departments.name) AS subdivision,
    subdivisions.code as subdivision_code,
    CONCAT(employees.last_name, ' ', employees.first_name, ' ', employees.patronymic_name) AS fio,
    users.name as responsible_person_fio,
    (select name from organizations  limit 1) as organizations_name
FROM stock_return
         LEFT JOIN employees ON employees.id = stock_return.employee_id
         LEFT JOIN subdivisions as ob1 ON ob1.id = employees.subdivision_id
         LEFT JOIN subdivisions as ob2 ON ob2.id = ob1.parent_subdivision_id
         LEFT JOIN subdivisions as ob3 ON ob3.id = ob2.parent_subdivision_id
         LEFT JOIN subdivisions as ob4 ON ob4.id = ob3.parent_subdivision_id
         LEFT JOIN subdivisions as ob5 ON ob5.id = ob4.parent_subdivision_id
         LEFT JOIN subdivisions as ob6 ON ob6.id = ob5.parent_subdivision_id
         LEFT JOIN subdivisions as ob7 ON ob7.id = ob6.parent_subdivision_id
         LEFT JOIN subdivisions as ob8 ON ob8.id = ob7.parent_subdivision_id
         LEFT JOIN subdivisions ON subdivisions.id = employees.subdivision_id
         LEFT JOIN departments ON departments.id = employees.department_id
         LEFT JOIN users ON users.id = stock_return.user_id

WHERE stock_return.id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="doc_date">
          <DataField>doc_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="subdivision">
          <DataField>subdivision</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="subdivision_code">
          <DataField>subdivision_code</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="fio">
          <DataField>fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="responsible_person_fio">
          <DataField>responsible_person_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="organizations_name">
          <DataField>organizations_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="doc_number">
          <DataField>doc_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
