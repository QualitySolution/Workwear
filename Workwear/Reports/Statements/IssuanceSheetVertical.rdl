  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    wear_cards.last_name,
    wear_cards.first_name,
    wear_cards.patronymic_name,
    wear_cards.personnel_number,
    IFNULL(nomenclature.name, protection_tools.name) as nomenclature,
    issuance_sheet_items.size,
    issuance_sheet_items.growth,
    case when operation_issued_by_employee.operation_write_off_id is null then nomenclature.ozm else concat(nomenclature.ozm, 'a')  end as nomenclature_number,
    measurement_units.name as units,
    measurement_units.okei,
    issuance_sheet_items.amount,
    issuance_sheet_items.start_of_use,
    issuance_sheet_items.lifetime
FROM
    issuance_sheet_items
    LEFT JOIN operation_issued_by_employee on operation_issued_by_employee.id = issuance_sheet_items.issued_operation_id
    LEFT JOIN wear_cards ON wear_cards.id = issuance_sheet_items.employee_id
    LEFT JOIN nomenclature ON issuance_sheet_items.nomenclature_id = nomenclature.id
    LEFT JOIN item_types ON nomenclature.type_id = item_types.id
    LEFT JOIN protection_tools ON protection_tools.id = issuance_sheet_items.protection_tools_id
    LEFT JOIN measurement_units ON (item_types.units_id = measurement_units.id OR protection_tools.units_id = measurement_units.id)
WHERE
    issuance_sheet_items.issuance_sheet_id = @id
    ORDER BY wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
