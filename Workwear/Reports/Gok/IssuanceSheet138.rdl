  <ReportParameters>
    <ReportParameter Name="dateStart">
      <DataType>String</DataType>
      <Nullable>False</Nullable>
      <AllowBlank>False</AllowBlank>
    </ReportParameter>
    <ReportParameter Name="dateEnd">
      <DataType>String</DataType>
      <Nullable>False</Nullable>
      <AllowBlank>False</AllowBlank>
    </ReportParameter>
  </ReportParameters>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    wear_cards.personnel_number,
    CONCAT_WS(' ', wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name) as fio,
    posts.name as post,
    SUM(IF(operation_issued_by_employee.protection_tools_id = 1, operation_issued_by_employee.issued, 0)) as issued_1,
    MAX(IF(operation_issued_by_employee.protection_tools_id = 1, norms_item.amount, 0)) as norm_1
FROM
    operation_issued_by_employee
    LEFT JOIN wear_cards ON wear_cards.id = operation_issued_by_employee.employee_id
	LEFT JOIN posts ON posts.id = wear_cards.post_id
    LEFT JOIN norms_item ON norms_item.id = operation_issued_by_employee.norm_item_id
WHERE
    operation_issued_by_employee.operation_time &gt; @dateStart AND operation_issued_by_employee.operation_time &lt; ADDDATE(@dateEnd, INTERVAL 1 DAY)
    AND operation_issued_by_employee.issued &gt; 0
    GROUP BY operation_issued_by_employee.employee_id
    ORDER BY wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateStart">
            <Value>=Parameters!dateStart</Value>
          </QueryParameter>
          <QueryParameter Name="dateEnd">
            <Value>=Parameters!dateEnd</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="personnel_number">
          <DataField>personnel_number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="fio">
          <DataField>fio</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="post">
          <DataField>post</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="issued_1">
          <DataField>issued_1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="norm_1">
          <DataField>norm_1</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
