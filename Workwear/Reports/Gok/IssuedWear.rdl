  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	DATE(operation_issued_by_employee.operation_time) as date,
    stock_expense_detail.stock_expense_id as docNum,
    IFNULL(nomenclature.name, protection_tools.name) as nomenclature,
    nomenclature.number as nomenclature_number,
    measurement_units.name as units,
    measurement_units.okei,
    operation_issued_by_employee.issued,
    CONCAT_WS('',
            CONCAT(wear_cards.last_name, ' '),
            IF(wear_cards.first_name = '',
                '',
                CONCAT(LEFT(wear_cards.first_name, 1), '.')),
            IF(wear_cards.patronymic_name = '',
                '',
                CONCAT(LEFT(wear_cards.patronymic_name, 1), '.'))) as fio
FROM
    operation_issued_by_employee
    LEFT JOIN wear_cards ON wear_cards.id = operation_issued_by_employee.employee_id
    LEFT JOIN nomenclature ON operation_issued_by_employee.nomenclature_id = nomenclature.id
    LEFT JOIN protection_tools ON protection_tools.id = operation_issued_by_employee.protection_tools_id
    LEFT JOIN item_types ON (nomenclature.type_id = item_types.id OR protection_tools.item_types_id = item_types.id)
    LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
    LEFT JOIN stock_expense_detail ON stock_expense_detail.employee_issue_operation_id = operation_issued_by_employee.id
WHERE
    operation_issued_by_employee.operation_time &gt; @dateStart AND operation_issued_by_employee.operation_time &lt; ADDDATE(@dateEnd, INTERVAL 1 DAY)
    AND operation_issued_by_employee.issued &gt; 0
    ORDER BY operation_issued_by_employee.operation_time, wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateStart">
            <Value>=Parameters!dateStart</Value>
          </QueryParameter>
          <QueryParameter Name="dateEnd">
            <Value>=Parameters!dateEnd</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="date">
          <DataField>date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="docNum">
          <DataField>docNum</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
