        </Field>
        <Field Name="amount_sum">
          <DataField>amount_sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="assessed_cost">
          <DataField>assessed_cost</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="sex">
          <DataField>sex</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
