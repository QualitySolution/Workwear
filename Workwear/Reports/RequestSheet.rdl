  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    protection_tools.name AS type_name,
    protection_tools.assessed_cost AS assessed_cost,
    protection_tools.id AS type_id,
    wear_cards.sex,
    heightAlias.name AS growth,
    sizeAlias.name AS size,
    measurement_units.name AS unit,
    measurement_units.okei AS okei,
    SUM( count_issue(
            norms_item.amount,
            CASE norms_item.period_type WHEN 'Year' THEN norms_item.period_count * 12 WHEN 'Month' THEN norms_item.period_count ELSE NULL END,
            wear_cards_item.next_issue,
            CONCAT(@begin_year, '-', @begin_month, '-01'),
            DATE_ADD(DATE_ADD(CONCAT(@end_year, '-', @end_month, '-01'),INTERVAL 1 MONTH), INTERVAL -1 DAY),
            MONTH(conditions.issuance_start),
            MONTH(conditions.issuance_end))
    ) AS amount_sum
FROM wear_cards_item
    LEFT JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
    LEFT JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
    LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
    LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
    LEFT JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
    LEFT JOIN size_types AS sizeTypeAlias ON sizeTypeAlias.id = item_types.size_type_id
    LEFT JOIN size_types AS heightTypeAlias ON heightTypeAlias.id = item_types.height_type_id
    LEFT JOIN wear_cards_sizes AS employeeSizeAlias ON (employeeSizeAlias.employee_id = wear_cards_item.wear_card_id AND employeeSizeAlias.size_type_id = sizeTypeAlias.id)
    LEFT JOIN sizes AS sizeAlias ON sizeAlias.id = employeeSizeAlias.size_id
    LEFT JOIN wear_cards_sizes AS employeeHeightAlias ON (employeeHeightAlias.employee_id = wear_cards_item.wear_card_id AND employeeHeightAlias.size_type_id = heightTypeAlias.id)
    LEFT JOIN sizes AS heightAlias ON heightAlias.id = employeeHeightAlias.size_id
    LEFT JOIN norm_conditions AS conditions ON conditions.id = norms_item.condition_id
    LEFT JOIN wear_cards_vacations ON wear_cards.id = wear_cards_vacations.wear_card_id
WHERE (-1 IN (@subdivisions) OR wear_cards.object_id IN (@subdivisions)) 
    AND ((@department_id = -1) OR wear_cards.department_id = @department_id) 
    AND wear_cards.dismiss_date IS NULL
    AND wear_cards_item.next_issue &lt; DATE_ADD(CONCAT(@end_year, '-', @end_month, '-01'), INTERVAL 1 MONTH) 
    AND( @issue_type = '' OR item_types.issue_type = @issue_type )
    AND ((-1 IN (@protectionTools)) OR (wear_cards_item.protection_tools_id IN (@protectionTools)))
    AND (@exclude_in_vacation = false OR (CURRENT_DATE() NOT BETWEEN wear_cards_vacations.begin_date AND wear_cards_vacations.end_date OR wear_cards_vacations.begin_date IS NULL))
GROUP BY
    wear_cards_item.protection_tools_id,
    IF(@show_sex, wear_cards.sex, NULL),
    size,
    growth
HAVING amount_sum &gt; 0
ORDER BY
    type_name,
    IF(@show_sex, wear_cards.sex, NULL),
    size,
    growth</CommandText>
        <QueryParameters>
          <QueryParameter Name="begin_year">
            <Value>={?begin_year}</Value>
          </QueryParameter>
          <QueryParameter Name="begin_month">
            <Value>={?begin_month}</Value>
          </QueryParameter>
          <QueryParameter Name="subdivisions">
            <Value>={?subdivisions}</Value>
          </QueryParameter>
          <QueryParameter Name="end_month">
            <Value>={?end_month}</Value>
          </QueryParameter>
          <QueryParameter Name="end_year">
            <Value>={?end_year}</Value>
          </QueryParameter>
          <QueryParameter Name="@issue_type">
            <Value>={?issue_type}</Value>
          </QueryParameter>
          <QueryParameter Name="protectionTools">
            <Value>={?protectionTools}</Value>
          </QueryParameter>
          <QueryParameter Name="@show_sex">
            <Value>={?show_sex}</Value>
          </QueryParameter>
          <QueryParameter Name="@exclude_in_vacation">
            <Value>={?exclude_in_vacation}</Value>
          </QueryParameter>
          <QueryParameter Name="@department_id">
            <Value>={?department_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="type_name">
          <DataField>type_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="type_id">
          <DataField>type_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
