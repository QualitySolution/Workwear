  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    protection_tools.name AS type_name,
    protection_tools.assessed_cost * SUM(norms_item.amount) AS assessed_cost,
    protection_tools.id AS type_id,
    CASE item_types.wear_category WHEN 'Wear' THEN wear_cards.wear_growth ELSE NULL
END AS growth,
CASE item_types.wear_category WHEN 'Wear' THEN wear_cards.size_wear WHEN 'Shoes' THEN wear_cards.size_shoes WHEN 'WinterShoes' THEN wear_cards.size_winter_shoes WHEN 'Headgear' THEN wear_cards.size_headdress WHEN 'Gloves' THEN wear_cards.size_gloves ELSE NULL
END AS size,
measurement_units.name AS unit,
measurement_units.okei AS okei,
SUM( count_issue(
    norms_item.amount, 
    CASE norms_item.period_type WHEN 'Year' THEN norms_item.period_count * 12 WHEN 'Month' THEN norms_item.period_count ELSE NULL END,
    MONTH(wear_cards_item.next_issue),
    YEAR(wear_cards_item.next_issue),
    @begin_month,
    @begin_year,
    @end_month,
    @end_year)
) AS amount_sum
FROM
    wear_cards_item
LEFT JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
LEFT JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
LEFT JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
WHERE
    (@subdivision = -1 OR wear_cards.object_id = @subdivision) 
    AND wear_cards.dismiss_date IS NULL 
    AND wear_cards_item.next_issue &lt; DATE_ADD(CONCAT(@end_year, '-', @end_month, '-01'), INTERVAL 1 MONTH)
GROUP BY
    protection_tools.id,
    size,
    growth
HAVING amount_sum &gt; 0
ORDER BY
    type_name,
    size,
    growth</CommandText>
        <QueryParameters>
          <QueryParameter Name="begin_year">
            <Value>={?begin_year}</Value>
          </QueryParameter>
          <QueryParameter Name="begin_month">
            <Value>={?begin_month}</Value>
          </QueryParameter>
          <QueryParameter Name="subdivision">
            <Value>={?subdivision}</Value>
          </QueryParameter>
          <QueryParameter Name="end_month">
            <Value>={?end_month}</Value>
          </QueryParameter>
