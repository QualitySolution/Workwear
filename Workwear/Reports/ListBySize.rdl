  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    CONCAT_WS(' ',
            wear_cards.last_name,
            wear_cards.first_name,
            wear_cards.patronymic_name) AS employee,
            IF(top1.id IS NULL, "-", size1.name) AS size_name_1,
            IF(top2.id IS NULL, "-", size2.name) AS size_name_2,
            IF(top3.id IS NULL, "-", size3.name) AS size_name_3,
            IF(top4.id IS NULL, "-", size4.name) AS size_name_4,
            IF(top5.id IS NULL, "-", size5.name) AS size_name_5,
            IF(top6.id IS NULL, "-", size6.name) AS size_name_6,
    personnel_number,
    sex
FROM
    wear_cards
LEFT JOIN wear_cards_sizes AS top1 ON wear_cards.id = top1.employee_id AND top1.size_type_id = @type_id_0
LEFT JOIN sizes AS size1 ON top1.size_id = size1.id
LEFT JOIN wear_cards_sizes AS top2 ON wear_cards.id = top2.employee_id AND top2.size_type_id = @type_id_1
LEFT JOIN sizes AS size2 ON top2.size_id = size2.id
LEFT JOIN wear_cards_sizes AS top3 ON wear_cards.id = top3.employee_id AND top3.size_type_id = @type_id_2
LEFT JOIN sizes AS size3 ON top3.size_id = size3.id
LEFT JOIN wear_cards_sizes AS top4 ON wear_cards.id = top4.employee_id AND top4.size_type_id = @type_id_3
LEFT JOIN sizes AS size4 ON top4.size_id = size4.id
LEFT JOIN wear_cards_sizes AS top5 ON wear_cards.id = top5.employee_id AND top5.size_type_id = @type_id_4
LEFT JOIN sizes AS size5 ON top5.size_id = size5.id
LEFT JOIN wear_cards_sizes AS top6 ON wear_cards.id = top6.employee_id AND top6.size_type_id = @type_id_5
LEFT JOIN sizes AS size6 ON top6.size_id = size6.id
WHERE
    dismiss_date IS NULL
ORDER BY last_name , first_name , patronymic_name</CommandText>
