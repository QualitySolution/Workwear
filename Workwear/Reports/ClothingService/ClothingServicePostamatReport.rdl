  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    clothing_service_states.comment,
    Count(*) as count
FROM clothing_service_states
WHERE 
    clothing_service_states.operation_time BETWEEN @start_date AND @finish_date
    AND clothing_service_states.state IN ('InReceiptTerminal', 'InDispenseTerminal')
GROUP BY clothing_service_states.comment</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="finish_date">
            <Value>=Parameters!finish_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="comment">
          <DataField>comment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count">
          <DataField>count</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
