<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2005/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description></Description>
  <Author>Тимирева Екатерина</Author>
  <PageHeight>60mm</PageHeight>
  <PageWidth>57mm</PageWidth>
  <Width>7.5in</Width>
  <TopMargin>1mm</TopMargin>
  <LeftMargin>1mm</LeftMargin>
  <RightMargin>1mm</RightMargin>
  <BottomMargin>1mm</BottomMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT csl.id AS 'claim_id', 
       barcodes.id AS 'barcode_id',
       csl.preferred_terminal_id,
       CONCAT_WS(' ', nomenclature.name, NULLIF(CONCAT('(', CONCAT_WS('/', wear_size.name, height_size.name) ,')'), '()')) AS nomenclature_name,
       employees.personnel_number AS 'tab_num',
       CONCAT_WS(' ', employees.last_name,employees.first_name, employees.patronymic_name) AS 'employee_name'
    FROM clothing_service_claim AS csl 
        JOIN barcodes ON csl.barcode_id = barcodes.id
        LEFT JOIN employees ON csl.employee_id = employees.id
        JOIN nomenclature ON barcodes.nomenclature_id = nomenclature.id
        LEFT JOIN sizes AS wear_size ON barcodes.size_id = wear_size.id
        LEFT JOIN sizes AS height_size ON barcodes.height_id = height_size.id
WHERE @barcode_id = barcodes.id AND @service_claim_id = csl.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="barcode_id">
            <Value>=Parameters!barcode_id</Value>
          </QueryParameter>
          <QueryParameter Name="service_claim_id">
            <Value>=Parameters!service_claim_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="claim_id">
          <DataField>claim_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="barcode_id">
          <DataField>barcode_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="preferred_terminal_id">
          <DataField>preferred_terminal_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tab_num">
          <DataField>tab_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="employee_name">
          <DataField>employee_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
