        </Field>
        <Field Name="repair">
          <DataField>repair</DataField>
          <rd:TypeName>System.Boolean</rd:TypeName>
        </Field>
        <Field Name="status">
          <DataField>status</DataField>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
