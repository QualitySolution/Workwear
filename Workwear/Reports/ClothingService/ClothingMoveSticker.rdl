    <ReportParameter Name="manufacturer_code">
      <DataType>String</DataType>
      <Nullable>false</Nullable>
      <AllowBlank>false</AllowBlank>
      <MultiValue>false</MultiValue>
      <Prompt></Prompt>
    </ReportParameter>
    <ReportParameter Name="number_system">
      <DataType>String</DataType>
      <Nullable>false</Nullable>
