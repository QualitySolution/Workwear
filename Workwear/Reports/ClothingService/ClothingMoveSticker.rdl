<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2005/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description></Description>
  <Author>Тимирева Екатерина</Author>
  <PageHeight>60mm</PageHeight>
  <PageWidth>57mm</PageWidth>
  <Width>7.5in</Width>
  <TopMargin>1mm</TopMargin>
  <LeftMargin>1mm</LeftMargin>
  <RightMargin>1mm</RightMargin>
  <BottomMargin>1mm</BottomMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT csl.id AS 'claim_id', 
       barcodes.id AS 'barcode_id',
       csl.preferred_terminal_id,
       CONCAT_WS(' ', nomenclature.name, NULLIF(CONCAT('(', CONCAT_WS('/', wear_size.name, height_size.name) ,')'), '()')) AS nomenclature_name,
       employees.personnel_number AS 'tab_num',
       CONCAT_WS(' ', employees.last_name,employees.first_name, employees.patronymic_name) AS 'employee_name'
    FROM clothing_service_claim AS csl 
        JOIN barcodes ON csl.barcode_id = barcodes.id
        LEFT JOIN employees ON csl.employee_id = employees.id
        JOIN nomenclature ON barcodes.nomenclature_id = nomenclature.id
        LEFT JOIN sizes AS wear_size ON barcodes.size_id = wear_size.id
        LEFT JOIN sizes AS height_size ON barcodes.height_id = height_size.id
WHERE @barcode_id = barcodes.id AND @service_claim_id = csl.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="barcode_id">
            <Value>=Parameters!barcode_id</Value>
          </QueryParameter>
          <QueryParameter Name="service_claim_id">
            <Value>=Parameters!service_claim_id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="claim_id">
          <DataField>claim_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="barcode_id">
          <DataField>barcode_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="preferred_terminal_id">
          <DataField>preferred_terminal_id</DataField>
          <rd:TypeName>System.UInt32</rd:TypeName>
        </Field>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="tab_num">
          <DataField>tab_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="employee_name">
          <DataField>employee_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <PageHeader>
    <Height>0.0pt</Height>
    <PrintOnFirstPage>true</PrintOnFirstPage>
    <PrintOnLastPage>true</PrintOnLastPage>
  </PageHeader>
  <Body>
    <Height>171.3pt</Height>
    <Columns >1</Columns>
    <ReportItems >
      <CustomReportItem Name="CustomReportItem2">
        <Type>BarCode EAN-13</Type>
        <Height>73.72pt</Height>
        <Width>106.10pt</Width>
        <CustomProperties>
          <CustomProperty>
            <Name>NumberSystem</Name>
            <Value>={?number_system}</Value>
          </CustomProperty>
          <CustomProperty>
            <Name>ManufacturerCode</Name>
            <Value>={?manufacturer_code}</Value>
          </CustomProperty>
          <CustomProperty>
            <Name>ProductCode</Name>
            <Value>={?product_code}</Value>
          </CustomProperty>
        </CustomProperties>
        <Left>21.7pt</Left>
        <Top>10.1pt</Top>
        <Style>
          <BorderStyle />
          <BorderColor />
          <BorderWidth />
        </Style>
        <ZIndex>0</ZIndex>
      </CustomReportItem>
      <Textbox Name="Textbox1">
        <Height>17.25pt</Height>
        <Width>135.00pt</Width>
        <Value>=КОРПУС + " " + {?preferred_terminal}</Value>
        <ZIndex>1</ZIndex>
        <Left>9.5pt</Left>
        <Top>88.0pt</Top>
        <Style>
          <BorderStyle />
          <BorderColor />
          <BorderWidth />
          <FontSize>14pt</FontSize>
          <TextAlign>Center</TextAlign>
          <VerticalAlign>Middle</VerticalAlign>
        </Style>
        <CanGrow>true</CanGrow>
      </Textbox>
      <Textbox Name="Textbox2">
        <Height>13.51pt</Height>
        <Width>138.00pt</Width>
        <Value>=First({nomenclature_name}, "Data")</Value>
        <ZIndex>1</ZIndex>
        <Left>5.20pt</Left>
        <Top>104.50pt</Top>
        <Style>
          <BorderStyle />
          <BorderColor />
          <BorderWidth />
          <TextAlign>Center</TextAlign>
          <VerticalAlign>Middle</VerticalAlign>
        </Style>
        <CanGrow>true</CanGrow>
      </Textbox>
      <Textbox Name="Textbox6">
        <Height>12.00pt</Height>
        <Width>135.00pt</Width>
        <Value>=First({tab_num}, "Data")</Value>
        <ZIndex>1</ZIndex>
        <Left>2.4pt</Left>
        <Top>121.2pt</Top>
        <Style>
          <BorderStyle />
          <BorderColor />
          <BorderWidth />
          <TextAlign>Center</TextAlign>
          <VerticalAlign>Middle</VerticalAlign>
        </Style>
        <CanGrow>true</CanGrow>
      </Textbox>
      <Textbox Name="Textbox7">
        <Height>12.00pt</Height>
        <Width>135.00pt</Width>
        <Value>=First({employee_name}, "Data")</Value>
        <ZIndex>1</ZIndex>
        <Left>2.4pt</Left>
        <Top>136.2pt</Top>
        <Style>
          <BorderStyle />
          <BorderColor />
          <BorderWidth />
          <TextAlign>Center</TextAlign>
          <VerticalAlign>Middle</VerticalAlign>
        </Style>
        <CanGrow>true</CanGrow>
      </Textbox>
    </ReportItems>
  </Body>
  <PageFooter>
    <Height>0.0pt</Height>
    <PrintOnFirstPage>false</PrintOnFirstPage>
    <PrintOnLastPage>false</PrintOnLastPage>
  </PageFooter>
  <DataElementName >Report</DataElementName>
  <DataElementStyle >AttributeNormal</DataElementStyle>
  <ReportParameters >
    <ReportParameter Name="barcode_id">
      <DataType>Integer</DataType>
      <DefaultValue>
