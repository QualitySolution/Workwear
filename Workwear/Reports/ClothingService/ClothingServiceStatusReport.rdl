  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    clothing_service_claim.id,
    barcodes.title AS barcode,
    nomenclature.name AS nomenclature,
    DATE_FORMAT(start.operation_time, '%d.%m.%Y') AS start_date,
    DATEDIFF(NOW(), start.operation_time) AS day_from_start,
    DATE_FORMAT(finish.operation_time, '%d.%m.%Y') AS finish_date,
    DATEDIFF(NOW(), finish.operation_time) AS day_into_disp_terminal,
    postomat_documents.terminal_location AS disp_location,
    postomat_document_items.cell_number AS disp_cell,
    employees.personnel_number AS tab_num,

    IF(@show_emoloyee, CONCAT_WS(' ', employees.last_name, employees.first_name,employees.patronymic_name), '') AS 'FIO',
    IF(@show_emoloyee, posts.name, '') AS post,
    IF(@show_emoloyee, subdivisions.name, '') AS subdivision,
    IF(@show_emoloyee, departments.name, '') AS department,
    IF(@show_phone, employees.phone_number, '') AS phone,
    IF(@show_comment, IFNULL(start.comment, ''), '') AS start_comment,
    IF(@show_comment, IFNULL(finish.comment, ''), '') AS finish_comment
FROM clothing_service_claim
         LEFT JOIN (select s.claim_id AS clid, MIN(s.id) AS st_id
                    FROM clothing_service_states AS s
                    GROUP BY s.claim_id
) AS t1 ON t1.clid = clothing_service_claim.id
         LEFT JOIN (select s.claim_id AS clid, MAX(s.id) AS fh_id
                    FROM clothing_service_states AS s
                    GROUP BY s.claim_id
) AS t2 ON t2.clid = clothing_service_claim.id
         LEFT JOIN clothing_service_states AS start ON start.id = t1.st_id
         LEFT JOIN clothing_service_states AS finish ON finish.id = t2.fh_id
         LEFT JOIN employees ON employees.id = clothing_service_claim.employee_id
         LEFT JOIN posts ON employees.post_id = posts.id
         LEFT JOIN subdivisions ON employees.subdivision_id = subdivisions.id
         LEFT JOIN departments ON employees.department_id = departments.id
         LEFT JOIN barcodes ON clothing_service_claim.barcode_id = barcodes.id
         LEFT JOIN nomenclature ON barcodes.nomenclature_id = nomenclature.id
         LEFT JOIN postomat_document_items ON clothing_service_claim.id = postomat_document_items.claim_id
         LEFT JOIN postomat_documents ON postomat_document_items.document_id = postomat_documents.id
WHERE (@status is null OR @status = '' OR finish.state = @status)
  AND (@show_closed OR NOT clothing_service_claim.is_closed)
  AND finish.operation_time BETWEEN @start_date AND @finish_date
ORDER BY start_date</CommandText>
        <QueryParameters>
          <QueryParameter Name="status">
            <Value>=Parameters!status</Value>
          </QueryParameter>
          <QueryParameter Name="show_comment">
            <Value>=Parameters!show_comment</Value>
          </QueryParameter>
          <QueryParameter Name="show_phone">
            <Value>=Parameters!show_phone</Value>
          </QueryParameter>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="finish_date">
            <Value>=Parameters!finish_date</Value>
          </QueryParameter>
          <QueryParameter Name="show_emoloyee">
            <Value>=Parameters!show_emoloyee</Value>
          </QueryParameter>
          <QueryParameter Name="show_closed">
            <Value>=Parameters!show_closed</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
