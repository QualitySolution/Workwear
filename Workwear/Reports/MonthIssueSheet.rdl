  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    CONCAT_WS(' ',
            wear_cards.last_name,
            wear_cards.first_name,
            wear_cards.patronymic_name) as employee,
    wear_cards.personnel_number,
    item_types.name as nomenclatureName,
    item_types.id as nomenclatureId,
    (CASE item_types.wear_category
        WHEN 'Wear' THEN wear_cards.size_wear
        WHEN 'Shoes' THEN wear_cards.size_shoes
        WHEN 'WinterShoes' THEN wear_cards.size_winter_shoes
        WHEN 'Gloves' THEN wear_cards.size_gloves
    END) as size,
   IF(item_types.wear_category = 'Wear',
            wear_cards.wear_growth, NULL) as growth,
    measurement_units.name as units,
    measurement_units.okei as okei,
    wear_cards_item.next_issue plan_date,
    norms_item.amount as amount,
    (CASE norms_item.period_type
        WHEN 'Month' THEN norms_item.period_count
        WHEN 'Year' THEN norms_item.period_count * 12
    END) as period
FROM
    wear_cards_item
        LEFT JOIN
    wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
        LEFT JOIN 
    protection_tools on protection_tools.id = wear_cards_item.protection_tools_id
        LEFT JOIN
    item_types ON item_types.id = protection_tools.item_types_id
        LEFT JOIN
    measurement_units ON measurement_units.id = item_types.units_id
        LEFT JOIN
    norms_item ON norms_item.id = wear_cards_item.norm_item_id
WHERE 
wear_cards.dismiss_date IS NULL AND 
(@facility = -1 OR wear_cards.object_id = @facility) AND
(@employee = -1 OR wear_cards.id = @employee) AND
wear_cards_item.next_issue &gt;= @start_date AND wear_cards_item.next_issue &lt;= @end_date
ORDER BY employee;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@start_date">
            <Value>={?start_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@end_date">
            <Value>={?end_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@facility">
            <Value>={?facility}</Value>
          </QueryParameter>
          <QueryParameter Name="@employee">
            <Value>={?employee}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="employee">
          <DataField>employee</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="personnel_number">
          <DataField>personnel_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclatureName">
          <DataField>nomenclatureName</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclatureId">
          <DataField>nomenclatureId</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="units">
          <DataField>units</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="okei">
          <DataField>okei</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="plan_date">
          <DataField>plan_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <TypeName>System.UInt16</TypeName>
        </Field>
        <Field Name="period">
          <DataField>period</DataField>
          <TypeName>System.UInt64</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Facility">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT objects.name FROM objects WHERE objects.id = @facility</CommandText>
        <QueryParameters>
          <QueryParameter Name="@facility">
            <Value>={?facility}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
