  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
n.name as nomenclature, 
opissue.issued as amount, 
CONCAT_WS(" ",
    wc.last_name,
    CONCAT(LEFT(wc.first_name, 1), "."),
    CONCAT(LEFT(wc.patronymic_name, 1), ".")
    )as FIO,
IF(wc.card_number IS NOT NULL, wc.card_number, wc.id) as personnel_number,
opissue.operation_time as issue_operation_time, 
opissue.auto_writeoff_date as writeoff_date_before, 
CONCAT(TRUNCATE(opinspect.wear_percent * 100, 0), "%") as percent_after,
CONCAT(TRUNCATE(opissue.wear_percent * 100, 0), "%") as percent_before,
IF(opinspect.issued = 0,
  IF(opissue.auto_writeoff_date IS NOT NULL AND opissue.auto_writeoff_date &gt; opinspect.operation_time,
   	"списать",
    "списать досрочно"),     
  IF(opinspect.auto_writeoff_date IS NULL,
   	"продлить до износа", 
   	CONCAT("продлить до ",DATE_FORMAT(opinspect.auto_writeoff_date, "%d.%m.%Y"),"г.")))
as writeoff_date_after,
items.cause
FROM stock_inspection_items as items
LEFT JOIN stock_inspection as doc on doc.id = items.stock_inspection_id
LEFT JOIN operation_issued_by_employee as opissue on opissue.id = items.operation_issue_id 
LEFT JOIN operation_issued_by_employee as opinspect on opinspect.id = items.new_operation_issue_id
LEFT JOIN wear_cards as wc on wc.id = opissue.employee_id
LEFT JOIN nomenclature as n ON n.id = opissue.nomenclature_id
WHERE doc.id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>=Parameters!id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="nomenclature">
          <DataField>nomenclature</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FIO">
          <DataField>FIO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="personnel_number">
          <DataField>personnel_number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="issue_operation_time">
          <DataField>issue_operation_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="writeoff_date_before">
          <DataField>writeoff_date_before</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="percent_after">
          <DataField>percent_after</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="percent_before">
          <DataField>percent_before</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="writeoff_date_after">
          <DataField>writeoff_date_after</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="cause">
          <DataField>cause</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Doc">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
doc.id,
users.name as user_name,
doc.date,
organizations.name as organization,
CONCAT_WS(" ",dir.surname, dir.name,dir.patronymic) as director_fio,
dir.position as dir_position,
chair.position as chair_position,
CONCAT_WS(" ",
    chair.surname,
    CONCAT(LEFT(chair.name, 1), "."),
    CONCAT(LEFT(chair.patronymic, 1), ".")
    )as chairman_fio
          
FROM stock_inspection as doc
LEFT JOIN users on doc.user_id = users.id
LEFT JOIN leaders as dir on doc.director_id = dir.id
LEFT JOIN leaders as chair on doc.chairman_id = chair.id
LEFT JOIN organizations on doc.organization_id = organizations.id

WHERE doc.id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="user_name">
          <DataField>user_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="date">
          <DataField>date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="director_fio">
          <DataField>director_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="dir_position">
          <DataField>dir_position</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="chair_position">
          <DataField>chair_position</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="chairman_fio">
          <DataField>chairman_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="organization">
          <DataField>organization</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Members">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
CONCAT_WS(" ",
    members.surname,
    CONCAT(LEFT(members.name, 1), "."),
    CONCAT(LEFT(members.patronymic, 1), ".")
    )as member_fio,
members.position

FROM stock_inspection_members
LEFT JOIN leaders as members on stock_inspection_members.member_id = members.id
WHERE stock_inspection_members.stock_inspection_id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="member_fio">
          <DataField>member_fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="position">
          <DataField>position</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
