  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    nomenclature.name as nomenclature_name,
    nomenclature.number as nomenclature_number,
    COALESCE(old_issuse.StartOfUse, old_duty_issued.start_of_use) as date_start,
    COALESCE(old_issuse.ExpiryByNorm, old_duty_issued.expiry_by_norm) as date_expiry,
    COALESCE(norms_item.period_count, duty_norm_items.period_count) AS period_num,
    IF(COALESCE(norms_item.period_count, duty_norm_items.period_count) BETWEEN 11 AND 19,
       CASE COALESCE(norms_item.period_type, duty_norm_items.period_type)
           WHEN 'Month' THEN 'месяцев'
           WHEN 'Year' THEN 'лет'
           END, IF(COALESCE(norms_item.period_count, duty_norm_items.period_count) % 10 = 1,
                   CASE COALESCE(norms_item.period_type, duty_norm_items.period_type)
                       WHEN 'Month' THEN 'месяц'
                       WHEN 'Year' THEN 'год'
                       END, IF(COALESCE(norms_item.period_count, duty_norm_items.period_count) % 10 BETWEEN 2 and 4,
                               CASE COALESCE(norms_item.period_type, duty_norm_items.period_type)
                                   WHEN 'Month' THEN 'месяца'
                                   WHEN 'Year' THEN 'года'
                                   END, CASE COALESCE(norms_item.period_type, duty_norm_items.period_type)
                                            WHEN 'Month' THEN 'месяцев'
                                            WHEN 'Year' THEN 'лет'
                                   END))) as period_type,
    CASE COALESCE(norms_item.period_type, duty_norm_items.period_type)
        WHEN 'Month' THEN 'время'
        WHEN 'Year' THEN 'время'
        WHEN 'Wearout' THEN 'До износа'
        WHEN 'Duty' THEN 'Дежурный'
        ELSE ' '
        END as period_specificity,
    COALESCE(old_issuse.issued, old_duty_issued.issued) as amount_issuse,
    COALESCE(this_issuse.returned, this_duty_issued.returned) as amount_return,
    COALESCE(this_issuse.wear_percent, this_duty_issued.wear_percent)  * 100 as wear_percent
FROM stock_return_items
         LEFT JOIN nomenclature on nomenclature.id = stock_return_items.nomenclature_id
         LEFT JOIN operation_issued_by_employee as this_issuse on this_issuse.id = stock_return_items.employee_issue_operation_id
         LEFT JOIN operation_issued_by_duty_norm as this_duty_issued ON this_duty_issued.id = stock_return_items.duty_norm_issue_operation_id
         LEFT JOIN operation_issued_by_employee as old_issuse on old_issuse.id = this_issuse.issued_operation_id
         LEFT JOIN operation_issued_by_duty_norm as old_duty_issued ON old_duty_issued.id = this_duty_issued.issued_operation_id
         LEFT JOIN norms_item on norms_item.id = old_issuse.norm_item_id
         LEFT JOIN duty_norm_items ON old_duty_issued.duty_norm_item_id = duty_norm_items.id
WHERE stock_return_items.stock_return_id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_number">
          <DataField>nomenclature_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="date_start">
          <DataField>date_start</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="date_expiry">
          <DataField>date_expiry</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="period_num">
          <DataField>period_num</DataField>
          <TypeName>System.Byte</TypeName>
        </Field>
        <Field Name="period_type">
          <DataField>period_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount_issuse">
          <DataField>amount_issuse</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="amount_return">
          <DataField>amount_return</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="wear_percent">
          <DataField>wear_percent</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="period_specificity">
          <DataField>period_specificity</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DocInfo">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    IFNULL(stock_return.doc_number, stock_return.id)
                                                                                                                       as doc_number,
    stock_return.date AS doc_date,
    CONCAT_WS(' - ', ob8.name, ob7.name, ob6.name, ob5.name, ob4.name, ob3.name, ob2.name, ob1.name, departments.name) AS subdivision,
    GROUP_CONCAT(CONCAT(employees.last_name, " ", employees.first_name, " ", employees.patronymic_name) SEPARATOR ", ") AS fio

FROM stock_return
         JOIN stock_return_items ON stock_return_items.stock_return_id = stock_return.id
         LEFT JOIN employees ON employees.id = stock_return_items.employee_id
         LEFT JOIN subdivisions as ob1 ON ob1.id = employees.subdivision_id
         LEFT JOIN subdivisions as ob2 ON ob2.id = ob1.parent_subdivision_id
         LEFT JOIN subdivisions as ob3 ON ob3.id = ob2.parent_subdivision_id
         LEFT JOIN subdivisions as ob4 ON ob4.id = ob3.parent_subdivision_id
         LEFT JOIN subdivisions as ob5 ON ob5.id = ob4.parent_subdivision_id
         LEFT JOIN subdivisions as ob6 ON ob6.id = ob5.parent_subdivision_id
         LEFT JOIN subdivisions as ob7 ON ob7.id = ob6.parent_subdivision_id
         LEFT JOIN subdivisions as ob8 ON ob8.id = ob7.parent_subdivision_id
         LEFT JOIN subdivisions ON subdivisions.id = employees.subdivision_id
         LEFT JOIN departments ON departments.id = employees.department_id

WHERE stock_return.id = @id
GROUP BY stock_return.id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="doc_date">
          <DataField>doc_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="subdivision">
          <DataField>subdivision</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="fio">
          <DataField>fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="doc_number">
          <DataField>doc_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
