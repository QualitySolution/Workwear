  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT
    protection_tools.name AS protection_tools_name,
    wear_cards_item.protection_tools_id,
    (
        CASE WHEN item_types.wear_category = 'Wear' THEN wear_cards.size_wear WHEN item_types.wear_category = 'Shoes' THEN wear_cards.size_shoes WHEN item_types.wear_category = 'Gloves' THEN wear_cards.size_gloves WHEN item_types.wear_category = 'Headgear' THEN wear_cards.size_headdress WHEN item_types.wear_category = 'WinterShoes' THEN wear_cards.size_winter_shoes WHEN item_types.wear_category = 'Mittens' THEN wear_cards.size_mittens ELSE NULL
    END
) AS size,
(
    CASE WHEN item_types.wear_category = 'Wear' THEN wear_cards.wear_growth ELSE NULL
END
) AS growth,
measurement_units.name AS unit,
SUM(norms_item.amount) AS amount,
(
    SELECT
        (
            SUM(
                CASE WHEN operation_warehouse.warehouse_receipt_id IS NOT NULL THEN operation_warehouse.amount ELSE 0
            END
        ) - SUM(
            CASE WHEN operation_warehouse.warehouse_expense_id IS NOT NULL THEN operation_warehouse.amount ELSE 0
        END
)
) AS SUM
FROM
    operation_warehouse
WHERE
    operation_warehouse.operation_time &lt;= @report_date AND operation_warehouse.nomenclature_id IN(
    SELECT
        protection_tools_nomenclature.nomenclature_id
    FROM
        protection_tools_nomenclature
    WHERE
        protection_tools_nomenclature.protection_tools_id = wear_cards_item.protection_tools_id OR protection_tools_nomenclature.protection_tools_id IN(
        SELECT
            protection_tools_replacement.protection_tools_analog_id
        FROM
            protection_tools_replacement
        WHERE
            protection_tools_replacement.protection_tools_id = wear_cards_item.protection_tools_id
    )
)
) AS in_stock
FROM
    wear_cards_item
JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
WHERE
    wear_cards.dismiss_date IS NULL 
    AND( @subdivision_id = -1 OR wear_cards.object_id = @subdivision_id )
    AND( @issue_type = "" OR item_types.issue_type = @issue_type )
    AND wear_cards_item.next_issue &lt; @report_date
GROUP BY
    wear_cards_item.protection_tools_id, 
    CASE WHEN item_types.wear_category = 'Wear' THEN wear_cards.size_wear WHEN item_types.wear_category = 'Shoes' THEN wear_cards.size_shoes WHEN item_types.wear_category = 'Gloves' THEN wear_cards.size_gloves WHEN item_types.wear_category = 'Headgear' THEN wear_cards.size_headdress WHEN item_types.wear_category = 'WinterShoes' THEN wear_cards.size_winter_shoes WHEN item_types.wear_category = 'Mittens' THEN wear_cards.size_mittens ELSE NULL END,
    CASE WHEN item_types.wear_category = 'Wear' THEN wear_cards.wear_growth ELSE NULL
END
ORDER BY
    protection_tools_name,
    size,
    growth;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@report_date">
            <Value>={?report_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@subdivision_id">
            <Value>={?subdivision_id}</Value>
          </QueryParameter>
          <QueryParameter Name="@issue_type">
            <Value>={?issue_type}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="protection_tools_name">
          <DataField>protection_tools_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="protection_tools_id">
          <DataField>protection_tools_id</DataField>
