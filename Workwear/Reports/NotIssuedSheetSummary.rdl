          <QueryParameter Name="@without_groups">
            <Value>={?without_groups}</Value>
          </QueryParameter>
          <QueryParameter Name="@department_id">
            <Value>={?department_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
