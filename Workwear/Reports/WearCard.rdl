  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    wear_cards_item.id,
    wear_cards_item.next_issue,
    IF(
        wear_cards_item.next_issue &lt;= CURDATE(), DATEDIFF(
            CURDATE(), wear_cards_item.next_issue),
            NULL
        ) AS delay,
        item_types.name AS type_name,
        nomenclature.name AS nomenclature_name,
        nomenclature_link.size,
        nomenclature_link.growth,
        measurement_units.name AS unit,
        CASE norms_item.period_type WHEN 'Year' THEN norms_item.period_count * 12 WHEN 'Month' THEN norms_item.period_count WHEN 'Shift' THEN norms_item.period_count / 21
        END AS life_in_months,
        wear_cards_item.last_issue,
        wear_cards_item.amount AS received
    FROM
        wear_cards_item
    LEFT JOIN item_types ON item_types.id = wear_cards_item.itemtype_id
    LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
    LEFT JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
    LEFT JOIN(
        SELECT
            nomenclature.type_id,
            operation_issued_by_employee.nomenclature_id,
            DATE(
                operation_issued_by_employee.operation_time
            ) AS DATE,
            operation_issued_by_employee.size,
            operation_issued_by_employee.growth
        FROM
            operation_issued_by_employee
        JOIN nomenclature ON operation_issued_by_employee.nomenclature_id = nomenclature.id
        WHERE
            operation_issued_by_employee.employee_id = @id
    ) AS nomenclature_link
ON
    nomenclature_link.date = wear_cards_item.last_issue AND nomenclature_link.type_id = wear_cards_item.itemtype_id
LEFT JOIN nomenclature ON nomenclature.id = nomenclature_link.nomenclature_id
WHERE
    wear_cards_item.wear_card_id = @id
GROUP BY
    wear_cards_item.id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="next_issue">
          <DataField>next_issue</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="delay">
          <DataField>delay</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="type_name">
