          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="life_in_months">
          <DataField>life_in_months</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="last_issue">
          <DataField>last_issue</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="received">
          <DataField>received</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Card">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
IF (wear_cards.card_number IS NULL, wear_cards.id, wear_cards.card_number) as id, 
wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name, 
wear_cards.hire_date, wear_cards.sex, wear_cards.photo,
wear_cards.wear_growth, wear_cards.size_wear, wear_cards.size_shoes,
wear_cards.size_headdress, wear_cards.size_gloves, posts.name AS post, 
CONCAT_WS(" ", leaders.surname, leaders.name, leaders.patronymic) as leader
FROM wear_cards
LEFT JOIN posts ON wear_cards.post_id = posts.id
LEFT JOIN leaders ON leaders.id = wear_cards.leader_id
WHERE wear_cards.id = @id;</CommandText>
