  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    protection_tools.name AS type_name,
    protection_tools.id AS type_id,
    IF(@show_sex, wear_cards.sex, NULL) as sex,
    heightAlias.name AS growth,
    sizeAlias.name AS size,
    measurement_units.name AS unit,
    measurement_units.okei AS okei,
    SUM(CASE norms_item.period_type
        WHEN 'Year' THEN norms_item.amount / norms_item.period_count
        WHEN 'Month' THEN norms_item.amount * 12 / norms_item.period_count
        ELSE NULL
    END) AS amount_sum
FROM
    wear_cards_item
        LEFT JOIN
    wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
    	LEFT JOIN
    protection_tools ON protection_tools.id =  wear_cards_item.protection_tools_id
        LEFT JOIN
    item_types ON item_types.id = protection_tools.item_types_id
        LEFT JOIN
    measurement_units ON item_types.units_id = measurement_units.id
        LEFT JOIN
    norms_item ON wear_cards_item.norm_item_id = norms_item.id
        LEFT JOIN 
	size_types AS sizeTypeAlias ON sizeTypeAlias.id = item_types.size_type_id
        LEFT JOIN 
	size_types AS heightTypeAlias ON heightTypeAlias.id = item_types.height_type_id
        LEFT JOIN 
	wear_cards_sizes AS employeeSizeAlias ON (employeeSizeAlias.employee_id = wear_cards_item.wear_card_id AND employeeSizeAlias.size_type_id = sizeTypeAlias.id)
        LEFT JOIN 
	sizes AS sizeAlias ON sizeAlias.id = employeeSizeAlias.size_id
        LEFT JOIN 
	wear_cards_sizes AS employeeHeightAlias ON (employeeHeightAlias.employee_id = wear_cards_item.wear_card_id AND employeeHeightAlias.size_type_id = heightTypeAlias.id)
        LEFT JOIN 
	sizes AS heightAlias ON heightAlias.id = employeeHeightAlias.size_id
WHERE wear_cards.dismiss_date IS NULL
    AND( @subdivision_id = -1 OR wear_cards.object_id = @subdivision_id )
    AND( @issue_type = "" OR item_types.issue_type = @issue_type ) 
GROUP BY protection_tools.id , sex, size, growth
ORDER BY type_name, size, growth, sex</CommandText>
        <QueryParameters>
          <QueryParameter Name="@subdivision_id">
            <Value>={?subdivision_id}</Value>
          </QueryParameter>
          <QueryParameter Name="@issue_type">
            <Value>={?issue_type}</Value>
          </QueryParameter>
          <QueryParameter Name="@show_sex">
            <Value>={?show_sex}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="type_name">
          <DataField>type_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="type_id">
          <DataField>type_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="sex">
          <DataField>sex</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="okei">
          <DataField>okei</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount_sum">
          <DataField>amount_sum</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Report" >
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT name as subdivision FROM objects WHERE id = @subdivision_id</CommandText>
        <QueryParameters>
          <QueryParameter Name="@subdivision_id">
            <Value>={?subdivision_id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="subdivision">
          <DataField>subdivision</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
