  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
nomenclature.name as nomenclature_name,
nomenclature.number as nomenclature_number,
old_issuse.StartOfUse as date_start,
old_issuse.ExpiryByNorm as date_expiry,
norms_item.period_count AS period_num,
norms_item.period_type AS period_type,
old_issuse.issued as amount_issuse,
this_issuse.returned as amount_return,
this_issuse.wear_percent * 100 as wear_percent

FROM stock_income_detail
LEFT JOIN nomenclature on nomenclature.id = stock_income_detail.nomenclature_id
LEFT JOIN operation_issued_by_employee as this_issuse on this_issuse.id = stock_income_detail.employee_issue_operation_id
LEFT JOIN operation_issued_by_employee as old_issuse on old_issuse.id = this_issuse.issued_operation_id
LEFT JOIN norms_item on norms_item.id = old_issuse.norm_item_id
WHERE stock_income_detail.stock_income_id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="nomenclature_number">
          <DataField>nomenclature_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="date_start">
          <DataField>date_start</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="date_expiry">
          <DataField>date_expiry</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="period_num">
          <DataField>period_num</DataField>
          <TypeName>System.Byte</TypeName>
        </Field>
        <Field Name="period_type">
          <DataField>period_type</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount_issuse">
          <DataField>amount_issuse</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="amount_return">
          <DataField>amount_return</DataField>
          <TypeName>System.Int32</TypeName>
        </Field>
        <Field Name="wear_percent">
          <DataField>wear_percent</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DocInfo">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
stock_income.date AS doc_date,
objects.name AS subdivision,
departments.name AS department,
CONCAT(wear_cards.last_name, " ", wear_cards.first_name, " ", wear_cards.patronymic_name) AS fio

FROM stock_income
LEFT JOIN wear_cards ON wear_cards.id = stock_income.wear_card_id
LEFT JOIN objects ON objects.id = wear_cards.object_id
LEFT JOIN departments ON departments.id = wear_cards.department_id

WHERE stock_income.id = @id</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>={?id}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="doc_date">
          <DataField>doc_date</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="subdivision">
          <DataField>subdivision</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="department">
          <DataField>department</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="fio">
          <DataField>fio</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
