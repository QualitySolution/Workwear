  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    CONCAT_WS(' ', employees.last_name, employees.first_name,employees.patronymic_name) AS FIO,
    employees.personnel_number AS tab,
    employees.phone_number AS phone,
    subdivisions.name AS subdivision,
    departments.name AS department,
    barcodes.title AS barcode,
    nomenclature.name AS nomenclature_name,
    SUM(clothing_service_claim.id IS NOT NULL) AS count_claim
FROM operation_issued_by_employee
    LEFT JOIN employees ON employees.id = operation_issued_by_employee.employee_id
    LEFT JOIN subdivisions ON employees.subdivision_id = subdivisions.id
    LEFT JOIN departments ON employees.department_id = departments.id
    LEFT JOIN nomenclature ON operation_issued_by_employee.nomenclature_id = nomenclature.id
    LEFT JOIN operation_barcodes ON operation_issued_by_employee.id = operation_barcodes.employee_issue_operation_id
    LEFT JOIN barcodes ON operation_barcodes.barcode_id = barcodes.id
    LEFT JOIN clothing_service_claim ON barcodes.id = clothing_service_claim.barcode_id
    LEFT JOIN (select s.claim_id AS clid, MIN(s.id) AS st_id FROM clothing_service_states AS s GROUP BY s.claim_id
            ) AS t1 ON t1.clid = clothing_service_claim.id
        LEFT JOIN clothing_service_states AS start_state ON start_state.id = t1.st_id
WHERE
    operation_issued_by_employee.auto_writeoff_date &gt; @start_date
    AND nomenclature.use_barcode
    AND barcodes.id IS NOT NULL
    AND (clothing_service_claim.id IS  NULL OR start_state.operation_time BETWEEN @start_date AND @finish_date) #
GROUP BY operation_barcodes.barcode_id
HAVING @show_zero OR count_claim &gt; 0
ORDER BY count_claim DESC , subdivision, department, FIO</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="finish_date">
            <Value>=Parameters!finish_date</Value>
          </QueryParameter>
          <QueryParameter Name="show_zero">
            <Value>=Parameters!show_zero</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
