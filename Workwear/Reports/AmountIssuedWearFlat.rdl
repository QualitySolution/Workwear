  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select
    if(@summary, NULL, objects.id) as subdivision_id,
    if(@summary, NULL, objects.name) as object,
    if(@summary, NULL, objects.code) as subdivision_code,
    if(@showCostCenter,IFNULL((SELECT CONCAT_WS(' ',cc.code, cc.name)  
                               FROM wear_cards_cost_allocation as wcca 
                                   JOIN cost_center as cc on wcca.cost_center_id = cc.id 
                               WHERE wear_cards.id = wcca.wear_card_id
                               LIMIT 1),
                             CONCAT_WS(' ', cost_center.code, cost_center.name)), NULL) as cost_center_name,
    IF(@byEmployee, wear_cards.personnel_number, NULL) AS personnel_number,
    IF(@byEmployee, CONCAT_WS(' ', wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name), NULL) AS employee,
    IF(@byEmployee, posts.name, NULL) AS post,
    IF(@byEmployee, IF(wear_cards.sex = 'M','М',IF(wear_cards.sex = 'F','Ж','-')) , NULL) AS sex,
    nomenclature.name as nomen, 
    sum(issued) as amount, 
    nomenclature.number, 
    if(@alternativeName, IFNULL(sizeAlias.alternative_name, sizeAlias.name), sizeAlias.name) as size, 
    heightAlias.name as growth, 
    if(@showOwners, owners.name, null) as owner_name,
    (nomenclature.sale_cost) AS cost,
    (sum(issued) * nomenclature.sale_cost) AS cost_sum,
    CASE norms_item.period_type
        WHEN 'Year' THEN norms_item.period_count * 12
        WHEN 'Month' THEN norms_item.period_count
        END AS wearing_period,
    GROUP_CONCAT(DATE_FORMAT(operation_issued_by_employee.operation_time, '%d.%m.%Y')) as operation_date,
    GROUP_CONCAT(IFNULL(issuance_sheet.doc_number, issuance_sheet.id)) as issuance_sheet,
    IF(@byOperation, (SELECT DATE_FORMAT(IF(MAX(op3.operation_time) &gt; MAX(op2.auto_writeoff_date), op3.auto_writeoff_date, op2.auto_writeoff_date), '%d.%m.%Y')
        FROM operation_issued_by_employee as op2
             LEFT JOIN operation_issued_by_employee as op3 on op3.issued_operation_id = op2.id
        WHERE op2.employee_id = operation_issued_by_employee.employee_id 
          AND op2.protection_tools_id = operation_issued_by_employee.protection_tools_id 
          AND op2.auto_writeoff_date &lt;= operation_issued_by_employee.operation_time )
    ,NULL) AS date_need,
    operation_issued_by_employee.comment AS operation_comment,
    IFNULL(stock_expense.comment, stock_collective_expense.comment) AS document_comment
FROM operation_issued_by_employee
         LEFT JOIN wear_cards on wear_cards.id = operation_issued_by_employee.employee_id
         LEFT JOIN objects on objects.id = wear_cards.object_id
         LEFT JOIN posts ON posts.id = wear_cards.post_id
         LEFT JOIN cost_center ON cost_center.id = posts.cost_center_id
         LEFT JOIN nomenclature on nomenclature.id = operation_issued_by_employee.nomenclature_id
         LEFT JOIN item_types ON item_types.id = nomenclature.type_id
         LEFT JOIN sizes as sizeAlias ON (sizeAlias.id = operation_issued_by_employee.size_id)
         LEFT JOIN sizes AS heightAlias ON (heightAlias.id = operation_issued_by_employee.height_id)
         LEFT JOIN operation_warehouse ON warehouse_operation_id = operation_warehouse.id
         LEFT JOIN owners ON operation_warehouse.owner_id = owners.id
         LEFT JOIN protection_tools ON protection_tools.id = operation_issued_by_employee.protection_tools_id
         LEFT JOIN norms_item ON norms_item.id = operation_issued_by_employee.norm_item_id
         LEFT JOIN issuance_sheet_items ON issuance_sheet_items.issued_operation_id = operation_issued_by_employee.id
         LEFT JOIN issuance_sheet ON issuance_sheet_items.issuance_sheet_id = issuance_sheet.id
         LEFT JOIN stock_expense_detail ON operation_issued_by_employee.id = stock_expense_detail.employee_issue_operation_id
         LEFT JOIN stock_expense ON stock_expense_detail.stock_expense_id = stock_expense.id
         LEFT JOIN stock_collective_expense_detail ON operation_issued_by_employee.id = stock_collective_expense_detail.employee_issue_operation_id
         LEFT JOIN stock_collective_expense ON stock_collective_expense_detail.stock_collective_expense_id = stock_collective_expense.id
WHERE operation_issued_by_employee.operation_time &gt;= CONVERT(@dateStart, DATE) and operation_issued_by_employee.operation_time &lt; DATE_ADD(CONVERT(@dateEnd, DATE), INTERVAL 1 DAY)
  AND NOT operation_issued_by_employee.manual_operation -- В отчете не показываем ручные операции. 
  AND (@issue_type = '' OR item_types.issue_type = @issue_type)
  and operation_issued_by_employee.returned = 0
  AND ((-1 IN (@subdivisions)) OR (@withoutsub AND objects.id IS NULL) OR (objects.id IN (@subdivisions)))
  and (@matchString = '' OR nomenclature.name LIKE CONCAT('%', @matchString, '%'))
  and (@noMatchString = '' OR nomenclature.name NOT LIKE CONCAT('%', @noMatchString, '%'))
  AND (@allOwners OR (@withoutOwner AND owners.id IS NULL) OR owners.id = @ownerId)
  AND (NOT @showOnlyWithoutNorm OR operation_issued_by_employee.protection_tools_id IS NULL)
AND ((-1 IN (@employee_groups_ids))
        OR(@without_groups AND NOT EXISTS
            (SELECT * FROM employee_group_items as gi
             WHERE gi.employee_id = operation_issued_by_employee.employee_id) )
        OR (EXISTS (SELECT * FROM employee_group_items as gi
                    WHERE gi.employee_id = operation_issued_by_employee.employee_id
                      AND gi.employee_group_id IN (@employee_groups_ids))) )
GROUP BY IF(@summary, NULL, objects.id), IF(@byEmployee, wear_cards.id, NULL), operation_issued_by_employee.nomenclature_id, IF(@bysize, size, NULL), IF(@bysize, growth, NULL), IF(@showOwners, owners.id, NULL), IF(@byOperation, operation_issued_by_employee.id, NULL)
ORDER BY CAST(subdivision_code AS UNSIGNED), employee, nomen, size, growth;</CommandText>
        <QueryParameters>
          <QueryParameter Name="dateStart">
            <Value>=Format({?dateStart}, "yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="dateEnd">
            <Value>=Format({?dateEnd}, "yyyy-MM-dd")</Value>
          </QueryParameter>
          <QueryParameter Name="withoutsub">
            <Value>={?withoutsub}</Value>
          </QueryParameter>
          <QueryParameter Name="subdivisions">
            <Value>={?subdivisions}</Value>
          </QueryParameter>
          <QueryParameter Name="summary">
