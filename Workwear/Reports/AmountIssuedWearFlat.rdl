          <QueryParameter Name="byEmployee">
            <Value>={?byEmployee}</Value>
          </QueryParameter>
          <QueryParameter Name="showOnlyWithoutNorm">
            <Value>={?showOnlyWithoutNorm}</Value>
          </QueryParameter>
          <QueryParameter Name="showCostCenter">
            <Value>={?showCostCenter}</Value>
          </QueryParameter>
          <QueryParameter Name="byOperation">
            <Value>={?byOperation}</Value>
          </QueryParameter>
          <QueryParameter Name="without_groups">
            <Value>={?without_groups}</Value>
          </QueryParameter>
          <QueryParameter Name="employee_groups_ids">
            <Value>={?employee_groups_ids}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
