  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <Timeout>300</Timeout>
        <CommandText>SELECT
    protection_tools.name AS protection_tools_name,
    protection_tools.assessed_cost * SUM(norms_item.amount) AS protection_tools_assessed_cost,
    wear_cards_item.protection_tools_id,
    if(@group_by_subdivision, objects.code, NULL) as subdivision_code,
    if(@group_by_subdivision, objects.id, NULL)as subdivision_id,
    objects.name as subdivision_name,
    GROUP_CONCAT( CONCAT_WS('',
            CONCAT(wear_cards.last_name, ' '),
            IF(wear_cards.first_name = '',
                NULL,
                CONCAT(LEFT(wear_cards.first_name, 1), '.')),
            IF(wear_cards.patronymic_name = '',
                NULL,
                CONCAT(LEFT(wear_cards.patronymic_name, 1), '.'))) SEPARATOR ', ' LIMIT 500) AS employees,
    wear_cards.sex,
    sizeTypeAlias.id as sizeTypeId,
    heightTypeAlias.id as heightTypeId,
    sizeAlias.name AS size,
    heightAlias.name AS growth,
    measurement_units.name AS unit,
    SUM(norms_item.amount) AS amount,
    IF(@show_stock,(
        SELECT(
            SUM(CASE WHEN (operation_warehouse.warehouse_receipt_id IS NOT NULL
                    AND(@all_warehouse OR operation_warehouse.warehouse_receipt_id = @warehouse_id) )
                THEN operation_warehouse.amount  
                ELSE 0
                END
            ) - SUM(
                CASE WHEN (operation_warehouse.warehouse_expense_id IS NOT NULL
                    AND(@all_warehouse OR operation_warehouse.warehouse_expense_id = @warehouse_id) )
                THEN operation_warehouse.amount
                ELSE 0
                END
        )) AS SUM
        FROM
            operation_warehouse
        WHERE
            (@all_warehouse OR operation_warehouse.warehouse_receipt_id = @warehouse_id OR operation_warehouse.warehouse_expense_id = @warehouse_id)
            AND operation_warehouse.operation_time &lt;= CONVERT(@report_date, DATE) 
            AND operation_warehouse.nomenclature_id IN(
            SELECT
                protection_tools_nomenclature.nomenclature_id
            FROM
                protection_tools_nomenclature
            JOIN nomenclature n ON n.id = protection_tools_nomenclature.nomenclature_id
            WHERE
                (@show_sex = 0 OR n.sex = 'Universal' OR (n.sex = 'Men' AND wear_cards.sex = 'M') OR (n.sex = 'Women' AND wear_cards.sex = 'F'))
                AND protection_tools_nomenclature.protection_tools_id = wear_cards_item.protection_tools_id
            )
            AND (sizeTypeAlias.id IS NULL OR operation_warehouse.size_id = employeeSizeAlias.size_id OR operation_warehouse.size_id IN (SELECT IF(size_suitable.size_id = employeeSizeAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeSizeAlias.size_id OR size_suitable.`size_suitable_id` = employeeSizeAlias.size_id))
            AND (heightTypeAlias.id IS NULL OR operation_warehouse.height_id = employeeHeightAlias.size_id OR operation_warehouse.height_id IN (SELECT IF(size_suitable.size_id = employeeHeightAlias.size_id, size_suitable.size_suitable_id, size_suitable.size_id) FROM `size_suitable` WHERE size_suitable.`size_id` = employeeHeightAlias.size_id OR size_suitable.`size_suitable_id` = employeeHeightAlias.size_id))
            AND ((NOT @hide_worn) OR (operation_warehouse.wear_percent = 0.0))
        )
        , null) AS in_stock
FROM
    wear_cards_item
JOIN wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
LEFT JOIN posts ON wear_cards.post_id = posts.id
LEFT JOIN objects ON wear_cards.object_id = objects.id OR wear_cards.object_id IS NULL AND posts.subdivision_id = objects.id
JOIN protection_tools ON protection_tools.id = wear_cards_item.protection_tools_id
LEFT JOIN item_types ON item_types.id = protection_tools.item_types_id
LEFT JOIN measurement_units ON item_types.units_id = measurement_units.id
LEFT JOIN wear_cards_vacations ON wear_cards.id = wear_cards_vacations.wear_card_id
LEFT JOIN size_types AS sizeTypeAlias ON sizeTypeAlias.id = item_types.size_type_id
LEFT JOIN size_types AS heightTypeAlias ON heightTypeAlias.id = item_types.height_type_id
LEFT JOIN wear_cards_sizes AS employeeSizeAlias ON (employeeSizeAlias.employee_id = wear_cards_item.wear_card_id AND employeeSizeAlias.size_type_id = sizeTypeAlias.id)
LEFT JOIN sizes AS sizeAlias ON sizeAlias.id = employeeSizeAlias.size_id
LEFT JOIN wear_cards_sizes AS employeeHeightAlias ON (employeeHeightAlias.employee_id = wear_cards_item.wear_card_id AND employeeHeightAlias.size_type_id = heightTypeAlias.id)
LEFT JOIN sizes AS heightAlias ON heightAlias.id = employeeHeightAlias.size_id
JOIN norms_item ON wear_cards_item.norm_item_id = norms_item.id
LEFT JOIN norm_conditions ON norms_item.condition_id = norm_conditions.id
LEFT JOIN employee_group_items ON wear_cards.id = employee_group_items.employee_id
WHERE
    wear_cards.dismiss_date IS NULL
    AND ((@without_groups AND employee_group_items.id IS NULL) OR (employee_group_items.employee_group_id IN (@employee_groups_ids)))
    AND (@exclude_in_vacation = false OR (CONVERT(@report_date, DATE) NOT BETWEEN wear_cards_vacations.begin_date AND wear_cards_vacations.end_date OR wear_cards_vacations.begin_date IS NULL))
    AND (@subdivision_id = -1 OR wear_cards.object_id = @subdivision_id )
    AND protection_tools.id IN (@protection_tools_ids)
    AND (@condition = false OR norm_conditions.issuance_start is null OR
       IF(SUBSTRING(norm_conditions.issuance_start,6) &lt; SUBSTRING(norm_conditions.issuance_end,6),
          SUBSTRING(@report_date,6) &gt;= SUBSTRING(norm_conditions.issuance_start,6) AND SUBSTRING(@report_date,6) &lt;= SUBSTRING(norm_conditions.issuance_end,6),
          SUBSTRING(@report_date,6) &gt;= SUBSTRING(norm_conditions.issuance_start,6) OR SUBSTRING(@report_date,6) &lt;= SUBSTRING(norm_conditions.issuance_end,6)))
    AND( @issue_type = '' OR item_types.issue_type = @issue_type )
    AND wear_cards_item.next_issue &lt; CONVERT(@report_date, DATE)
    AND (@exclude_before = '' OR CONVERT(@exclude_before, DATE) &lt; wear_cards_item.next_issue)
GROUP BY
    IF(@group_by_subdivision, objects.id, NULL),
    wear_cards_item.protection_tools_id,
    IF(@show_sex, wear_cards.sex, NULL),
    size,
    growth
ORDER BY
    protection_tools_name,
    IF(@show_sex, wear_cards.sex, NULL),
    size,
    growth;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@report_date">
            <Value>={?report_date}</Value>
          </QueryParameter>
          <QueryParameter Name="@subdivision_id">
            <Value>={?subdivision_id}</Value>
          </QueryParameter>
          <QueryParameter Name="@issue_type">
            <Value>={?issue_type}</Value>
          </QueryParameter>
          <QueryParameter Name="@show_sex">
