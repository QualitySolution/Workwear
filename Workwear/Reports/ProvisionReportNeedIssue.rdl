      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>-- потребности
SELECT
    wear_cards.personnel_number as 'Табельный номер',
    Concat_ws(' ', wear_cards.last_name,wear_cards.first_name, wear_cards.patronymic_name) as "ФИО",
    IF(@show_dismissed,DATE_FORMAT(wear_cards.dismiss_date, '%d.%m.%Y'), '-') as 'Дата увольнения',
    IF(@group_by_subdivision, wear_cards.object_id, NULL) AS 'subdivision_id',
    objects.name as "Подразделение",
    departments.name as "Отдел",
    posts.name as 'Должность',
    protection_tools.name as "Номенклатура нормы",
    '-' as 'Наименование СИЗ',
    '' as "Модель, марка, артикул, класс защиты СИЗ, дерматологических СИЗ",
    '-' as 'Дата_строка',
    '-' as 'Дата',
    '' as 'Количество',
    CASE
        WHEN norms_item.period_type = 'Shift' THEN CONCAT(norms_item.amount, 'на смену')
        WHEN norms_item.period_type = 'Wearout' THEN ',До износа'
        WHEN norms_item.period_type = 'Duty' THEN 'Деж.'
        WHEN norms_item.period_type = 'Year' THEN CONCAT(norms_item.amount, ' на ', norms_item.period_count,
                                                         CASE
                                                             WHEN norms_item.period_count BETWEEN 11 AND 19 THEN ' лет'
                                                             WHEN norms_item.period_count % 10 = 1 THEN ' год'
                                                             WHEN norms_item.period_count % 10 BETWEEN 2 and 4 THEN ' года'
                                                             ELSE ' лет'
                                                             END)
        WHEN norms_item.period_type = 'Month' THEN CONCAT(norms_item.amount, ' на ',norms_item.period_count,
                                                          CASE
                                                              WHEN norms_item.period_count BETWEEN 11 AND 19 THEN ' месяцев'
                                                              WHEN norms_item.period_count % 10 = 1 THEN ' месяц'
                                                              WHEN norms_item.period_count % 10 BETWEEN 2 and 4 THEN ' месяца'
                                                              ELSE ' месяцев'
                                                              END)
        END as "Срок носки"
FROM `wear_cards_item`
         left JOIN wear_cards on wear_cards.id = wear_cards_item.wear_card_id
         LEFT JOIN objects on objects.id = wear_cards.object_id
         LEFT JOIN departments on departments.id = wear_cards.department_id
         LEFT JOIN protection_tools on protection_tools.id = wear_cards_item.protection_tools_id
         LEFT JOIN norms_item on norms_item.id = wear_cards_item.norm_item_id
         LEFT JOIN posts on posts.id = wear_cards.post_id
WHERE(IF(!@show_dismissed, wear_cards.dismiss_date is null,
         wear_cards.dismiss_date is null or wear_cards.dismiss_date is not null))
-- операции выдачи
UNION
SELECT
    wear_cards.personnel_number as "Табельный номер",
    CONCAT_WS(' ' ,wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name) as 'ФИО',
    IF(@show_dismissed,DATE_FORMAT(wear_cards.dismiss_date, '%d.%m.%Y'), '-') as 'Дата увольнения',
    IF(@group_by_subdivision, wear_cards.object_id, NULL) AS 'subdivision_id',
    objects.name as "Подразделение",
    departments.name as "Отдел",
    posts.name as "Должность",
    protection_tools.name as 'Номенклатура нормы',
    nomenclature.name as 'Наименование СИЗ',
    nomenclature.number as "Модель, марка, артикул, класс защиты СИЗ, дерматологических СИЗ",
    DATE_FORMAT(o1.operation_time, '%d.%m.%Y') as 'Дата_строка',
    o1.operation_time as 'Дата',
    o1.issued as 'Количество',
    '-' as 'Срок носки'
FROM operation_issued_by_employee o1
         LEFT JOIN protection_tools on o1.protection_tools_id = protection_tools.id
         LEFT JOIN nomenclature on o1.nomenclature_id = nomenclature.id
    #LEFT JOIN warehouse on operation_warehouse.warehouse_expense_id = warehouse.id   
         LEFT JOIN wear_cards on o1.employee_id = wear_cards.id
         LEFT JOIN posts on posts.id = wear_cards.post_id
         LEFT JOIN objects on objects.id = wear_cards.object_id
         LEFT JOIN departments on departments.id = wear_cards.department_id
         LEFT JOIN operation_issued_by_employee o2 on o1.id = o2.issued_operation_id
WHERE o1.issued != 0 and (o1.auto_writeoff_date &gt; (select now())) and 
      (IF(!@show_dismissed, wear_cards.dismiss_date is null, wear_cards.dismiss_date is null or wear_cards.dismiss_date 
          is not null))
  and o1.issued_operation_id is null
ORDER BY `ФИО`, `Номенклатура нормы`, `Дата`;</CommandText>
        <QueryParameters>
          <QueryParameter Name="show_dismissed">
            <Value>={?show_dismissed}</Value>
          </QueryParameter>
          <QueryParameter Name="group_by_subdivision">
            <Value>={?group_by_subdivision}</Value>
          </QueryParameter>
          <QueryParameter Name="protection_tools_ids">
            <Value>={?protection_tools_ids}</Value>
          </QueryParameter>
