  <DataSets>
    <DataSet Name="Data">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
LEFT(title, 2) AS number_system,
SUBSTRING(title, 3, 5) AS manufacture_code,
SUBSTRING(title, 8, 5) AS product_code,
fractional,
operation_issued_by_employee.operation_time AS operation_date,
objects.name AS subdivision,
organizations.name AS organization,
CONCAT_WS(' ', wear_cards.last_name, wear_cards.first_name, wear_cards.patronymic_name) AS employee,
nomenclature.id AS nomenclature_id,
nomenclature.name AS nomenclature,
operation_issued_by_employee.id AS operation_number
FROM `barcodes`
LEFT JOIN operation_issued_by_employee ON operation_issued_by_employee.id = employee_issue_operation_id
LEFT JOIN wear_cards ON wear_cards.id = operation_issued_by_employee.employee_id
LEFT JOIN objects ON objects.id = wear_cards.object_id
LEFT JOIN nomenclature ON nomenclature.id = operation_issued_by_employee.nomenclature_id
LEFT JOIN stock_expense_detail ON stock_expense_detail.employee_issue_operation_id = operation_issued_by_employee.id
LEFT JOIN stock_expense ON stock_expense.id = stock_expense_detail.stock_expense_id
LEFT JOIN users ON users.id = stock_expense.user_id
LEFT JOIN user_settings ON user_settings.user_id = users.id
LEFT JOIN organizations ON organizations.id = user_settings.default_organization_id
WHERE barcodes.id IN (@barcodes)</CommandText>
        <QueryParameters>
          <QueryParameter Name="barcodes">
            <Value>={?barcodes}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
