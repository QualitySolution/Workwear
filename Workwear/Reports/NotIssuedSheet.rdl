  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    wear_cards_item.id,
    CONCAT_WS(' ',
            wear_cards.last_name,
            wear_cards.first_name,
            wear_cards.patronymic_name) AS employee,
    wear_cards.personnel_number,
    wear_cards_item.next_issue,
    IF(wear_cards_item.next_issue &lt;= @report_date,
        DATEDIFF(@report_date, wear_cards_item.next_issue),
        NULL) AS delay,
    item_types.name AS type_name,
    item_types.id AS type_id,
    nomenclature.name AS nomenclature_name,
    (case when item_types.wear_category = 'Wear' then wear_cards.size_wear else 
    	(case when item_types.wear_category = 'Shoes' then wear_cards.size_shoes else 
    		(case when item_types.wear_category = 'Gloves' then wear_cards.size_gloves else 
    			(case when item_types.wear_category = 'Headgear' then wear_cards.size_headdress else 
    				(case when item_types.wear_category = 'WinterShoes' then wear_cards.size_winter_shoes else '' end) 
    			end) 
    		end) 
    	end) 
    end) as size,
    (case when item_types.wear_category = 'Wear' then wear_cards.wear_growth else '' end) as growth,
    measurement_units.name AS unit,
    measurement_units.okei AS okei,
    norms_item.amount AS amount_by_norm,
    (SELECT 
            MAX(stock_expense.date)
        FROM
            stock_expense_detail
                LEFT JOIN
            stock_expense ON stock_expense.id = stock_expense_detail.stock_expense_id
                LEFT JOIN
            nomenclature ON stock_expense_detail.nomenclature_id = nomenclature.id
        WHERE
            nomenclature.type_id = wear_cards_item.itemtype_id
                AND stock_expense.wear_card_id = wear_cards_item.wear_card_id) AS last_issue,
    wear_cards_item.amount AS received,
    (SELECT
          (sum(case when stock.warehouse_receipt_id is not null then stock.amount else 0 end) -
           sum(case when stock.warehouse_expense_id is not null then stock.amount else 0 end)) as sum
      FROM (
        SELECT operation_warehouse.nomenclature_id, operation_warehouse.amount, 
    			operation_warehouse.warehouse_receipt_id, operation_warehouse.warehouse_expense_id, 
    			wear_cards_item.id as wear_cards_item_id, nomenclature.type_id
        FROM operation_warehouse
        JOIN nomenclature on operation_warehouse.nomenclature_id = nomenclature.id
        JOIN wear_cards_item on wear_cards_item.itemtype_id = nomenclature.type_id
        GROUP by operation_warehouse.nomenclature_id, operation_warehouse.id) as stock
     WHERE  type_id = nomenclature.type_id
     GROUP BY stock.nomenclature_id) AS in_stock
FROM
    wear_cards_item
        JOIN
    wear_cards ON wear_cards.id = wear_cards_item.wear_card_id
        JOIN
    nomenclature ON nomenclature.type_id = wear_cards_item.itemtype_id
        JOIN
    item_types ON item_types.id = wear_cards_item.itemtype_id
        JOIN
    measurement_units ON item_types.units_id = measurement_units.id
        JOIN
    norms_item ON wear_cards_item.norm_item_id = norms_item.id
WHERE wear_cards.dismiss_date IS NULL AND (
    NOT @onlyMissing
        OR wear_cards_item.next_issue &lt; @report_date)
        group by wear_cards_item.wear_card_id, wear_cards_item.itemtype_id, wear_cards_item.norm_item_id
HAVING @onlyMissing
    OR (received IS NULL
    OR received &lt; amount_by_norm)
ORDER BY employee, type_name;</CommandText>
        <QueryParameters>
          <QueryParameter Name="@onlyMissing">
            <Value>={?only_missing}</Value>
          </QueryParameter>
          <QueryParameter Name="@report_date">
            <Value>={?report_date}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="employee">
          <DataField>employee</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="personnel_number">
          <DataField>personnel_number</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="next_issue">
          <DataField>next_issue</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="delay">
          <DataField>delay</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
        <Field Name="type_name">
          <DataField>type_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="type_id">
          <DataField>type_id</DataField>
          <TypeName>System.UInt32</TypeName>
        </Field>
        <Field Name="nomenclature_name">
          <DataField>nomenclature_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="size">
          <DataField>size</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="growth">
          <DataField>growth</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="unit">
          <DataField>unit</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="okei">
          <DataField>okei</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount_by_norm">
          <DataField>amount_by_norm</DataField>
          <TypeName>System.UInt16</TypeName>
        </Field>
        <Field Name="last_issue">
          <DataField>last_issue</DataField>
          <TypeName>System.DateTime</TypeName>
        </Field>
        <Field Name="received">
          <DataField>received</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
        <Field Name="in_stock">
          <DataField>in_stock</DataField>
          <TypeName>System.Decimal</TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
