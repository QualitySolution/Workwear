    <DataSet Name="Name">
      <Query >
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    CASE
        WHEN (MIN(start_period) = MAX(end_period)) THEN
            DATE_FORMAT(MIN(start_period),'%d.%m.%Y')
        ELSE
            CONCAT(DATE_FORMAT(MIN(start_period),'%d.%m.%Y'), ' - ', DATE_FORMAT(MAX(end_period),'%d.%m.%Y'))
        END AS shipment_period
FROM (
SELECT start_period, end_period
    FROM shipment
    JOIN shipment_items ON shipment.id = shipment_items.shipment_id
    WHERE ((-1 IN (@shipment_ids)) OR shipment.id IN (@shipment_ids))
      AND ((-1 IN (@nomenclature_ids)) OR shipment_items.nomenclature_id IN (@nomenclature_ids))
    GROUP BY shipment.id, shipment.start_period, shipment.end_period) q1;</CommandText>
        <QueryParameters>
          <QueryParameter Name="shipment_ids">
            <Value>={?shipment_ids}</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_ids">
            <Value>={?nomenclature_ids}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields >
        <Field Name="shipment_period">
          <DataField>shipment_period</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
