  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    shipment.id as doc_num,
    nomenclature.number as num,
    nomenclature.name as nom_name,
    wear_size.name as size_name,
    height_size.name as height_name,
    CONCAT_WS(' ', shipment_items.quantity, measurement_units.name) as requested,
    CONCAT_WS(' ', shipment_items.ordered, measurement_units.name) as ordered,
    shipment_items.cost,
    shipment_items.cost * shipment_items.quantity as total_requested,
    shipment_items.diff_cause,
    CONCAT_WS(' ', shipment_items.received,measurement_units.name) as received,
    shipment_items.comment
    FROM shipment
    JOIN shipment_items ON shipment.id = shipment_items.shipment_id
    JOIN nomenclature ON shipment_items.nomenclature_id = nomenclature.id
    JOIN item_types ON nomenclature.type_id = item_types.id
    JOIN measurement_units ON item_types.units_id = measurement_units.id
    LEFT JOIN sizes wear_size ON shipment_items.size_id = wear_size.id
    LEFT JOIN sizes height_size ON shipment_items.height_id = height_size.id
    WHERE ((-1 IN (@shipment_ids)) OR shipment.id IN (@shipment_ids))
      AND ((-1 IN (@nomenclature_ids)) OR shipment_items.nomenclature_id IN (@nomenclature_ids))
    ORDER BY `doc_num`, `nom_name`, `size_name`, `height_name`;</CommandText>
        <QueryParameters>
          <QueryParameter Name="shipment_ids">
            <Value>={?shipment_ids}</Value>
          </QueryParameter>
          <QueryParameter Name="nomenclature_ids">
            <Value>={?nomenclature_ids}</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
