  <DataSets>
    <DataSet Name="Card">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    IFNULL(wear_cards.card_number, wear_cards.id) AS id,
    wear_cards.personnel_number,
    wear_cards.last_name,
    wear_cards.first_name,
    wear_cards.patronymic_name,
    wear_cards.hire_date,
    wear_cards.change_of_position_date,
    wear_cards.sex,
    posts.name AS post,
    CONCAT_WS('',
            CONCAT(leaders.surname, ' '),
            IF(leaders.name = '',
                NULL,
                CONCAT(LEFT(leaders.name, 1), '.')),
            IF(leaders.patronymic = '',
                NULL,
                CONCAT(LEFT(leaders.patronymic, 1), '.'))) AS leader,
    objects.name AS subdivision,
    (SELECT 
            GROUP_CONCAT(DISTINCT CONCAT_WS(' ',
                            regulations.name,
                            CONCAT('от ',
                                    DATE_FORMAT(regulations.doc_date, '%d.%m.%Y')),
                            CONCAT('№ ', regulations.number))
                    SEPARATOR ', ')
        FROM
            wear_cards_norms
                LEFT JOIN
            norms ON norms.id = wear_cards_norms.norm_id
                LEFT JOIN
            regulations ON regulations.id = norms.regulations_id
        WHERE
            wear_cards_norms.wear_card_id = wear_cards.id) AS regulations
FROM
    wear_cards
        LEFT JOIN
    posts ON wear_cards.post_id = posts.id
        LEFT JOIN
    leaders ON leaders.id = wear_cards.leader_id
        LEFT JOIN
    objects ON objects.id = wear_cards.object_id
WHERE
    wear_cards.id = @id;</CommandText>
        <QueryParameters>
          <QueryParameter Name="id">
            <Value>=Parameters!id</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
