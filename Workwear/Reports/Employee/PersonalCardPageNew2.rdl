  <DataElementName>Report</DataElementName>
  <DataElementStyle>AttributeNormal</DataElementStyle>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>(SELECT
    IFNULL(nomenclature.name, protection_tools.name)
        AS name_siz,
    nomenclature.number
        AS nom_number,
    DATE_FORMAT(operation_issued_by_employee.operation_time,'%d.%m.%Y'
    ) AS expense_date,
    operation_issued_by_employee.issued
        AS expense_quantity,
    IF(protection_tools.dermal_ppe,'Лично','')
        AS dermal, #'' (пустая строка) используется в проверках
    IFNULL(CONCAT(operation_issued_by_employee.sign_key, DATE_FORMAT(operation_issued_by_employee.sign_timestamp, ' %d.%m.%Y %H:%i:%s')),
           CONCAT('Ведомость №', IFNULL(issuance_sheet.doc_number, issuance_sheet.id))
        ) AS issue_sign,
    CONCAT_WS('\n', GROUP_CONCAT(DATE_FORMAT(returned.date, '%d.%m.%Y') SEPARATOR '\n'),
              IF(operation_issued_by_employee.auto_writeoff_date &lt;= CURDATE()
                     AND operation_issued_by_employee.issued - IFNULL(SUM(returned.quantity), 0) &gt; 0,
                 DATE_FORMAT(operation_issued_by_employee.auto_writeoff_date,'%d.%m.%Y'),
                 NULL)
        ) AS returned_date,
    CONCAT_WS('\n', GROUP_CONCAT(returned.quantity SEPARATOR '\n'),
              IF(operation_issued_by_employee.auto_writeoff_date &lt;= CURDATE()
                     AND operation_issued_by_employee.issued - IFNULL(SUM(returned.quantity), 0) &gt; 0,
                 operation_issued_by_employee.issued - IFNULL(SUM(returned.quantity),0),
                 NULL)
        ) AS returned_quantity,
    IF(@typeSignature='Рукописная', null, 
        GROUP_CONCAT(IFNULL(CONCAT('Возврат №',IFNULL(returned.return_number, returned.return_doc_id)), '')
                  SEPARATOR '\n'))
        AS returned_sign,
    GROUP_CONCAT(IFNULL(CONCAT('№',IFNULL(returned.writeoff_number, returned.writeoff_doc_id)),'')
                 SEPARATOR '\n')
        AS returned_doc
FROM
    operation_issued_by_employee
        LEFT JOIN nomenclature ON operation_issued_by_employee.nomenclature_id = nomenclature.id
        LEFT JOIN protection_tools ON protection_tools.id = operation_issued_by_employee.protection_tools_id
        LEFT JOIN operation_warehouse ON operation_issued_by_employee.warehouse_operation_id = operation_warehouse.id
        LEFT JOIN issuance_sheet_items ON issuance_sheet_items.issued_operation_id = operation_issued_by_employee.id
        LEFT JOIN issuance_sheet ON issuance_sheet_items.issuance_sheet_id = issuance_sheet.id
        LEFT JOIN(
            SELECT
                return_detail.issued_operation_id,
                return_detail.operation_time AS DATE,
                return_detail.returned AS quantity,
                return_detail.sign_key,
                return_detail.sign_timestamp,
                stock_write_off_detail.stock_write_off_id AS writeoff_doc_id,
                stock_income_detail.stock_income_id AS return_doc_id,
                stock_income.doc_number AS return_number,
                stock_write_off.doc_number AS writeoff_number       
            FROM
                operation_issued_by_employee AS return_detail
                LEFT JOIN stock_write_off_detail on return_detail.id = stock_write_off_detail.employee_issue_operation_id
                LEFT JOIN stock_income_detail on return_detail.id = stock_income_detail.employee_issue_operation_id
                LEFT JOIN stock_income ON stock_income.id = stock_income_detail.stock_income_id
                LEFT JOIN stock_write_off ON stock_write_off.id = stock_write_off_detail.stock_write_off_id
            WHERE
                    return_detail.employee_id = @id AND return_detail.returned &gt; 0
        ) AS returned ON returned.issued_operation_id = operation_issued_by_employee.id
WHERE
    operation_issued_by_employee.employee_id = @id AND operation_issued_by_employee.issued &gt; 0
GROUP BY
    operation_issued_by_employee.id
ORDER BY
    operation_issued_by_employee.operation_time)

