        <Field Name="type_name">
          <DataField>type_name</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="ton_paragraph">
          <DataField>ton_paragraph</DataField>
          <TypeName>System.String</TypeName>
        </Field>
        <Field Name="amount">
