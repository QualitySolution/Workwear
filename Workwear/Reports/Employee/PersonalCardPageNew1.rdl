        </Field>
        <Field Name="unit_on_period">
          <DataField>unit_on_period</DataField>
          <TypeName>System.String</TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Sizes">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    size_types.name AS sizeName,
    sizes.name AS sizeValue
FROM size_types
    LEFT JOIN wear_cards_sizes ON wear_cards_sizes.employee_id = @id AND wear_cards_sizes.size_type_id = size_types.id
    LEFT JOIN sizes ON wear_cards_sizes.size_id = sizes.id
WHERE size_types.use_in_employee = true
ORDER BY size_types.position
LIMIT 10</CommandText>
        <QueryParameters>
